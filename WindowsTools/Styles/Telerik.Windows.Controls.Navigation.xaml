        <Setter Property="FocusVisualStyle" Value="{x:Null}" />
        <Setter Property="BorderThickness" Value="0" />
        <Setter Property="Padding" Value="5,0" />
        <Setter Property="MinHeight" Value="24" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
    </Style>
    <Style BasedOn="{StaticResource RadBreadcrumbStyle}" TargetType="telerikNavigation:RadBreadcrumb" />
    <Style BasedOn="{StaticResource RadBreadcrumbBarStyle}" TargetType="telerikBreadcrumb:RadBreadcrumbBar" />
    <Style BasedOn="{StaticResource RadBreadcrumbBarItemStyle}" TargetType="telerikBreadcrumb:RadBreadcrumbBarItem" />
    <Style
        x:Key="DesktopAlertButtonsBaseStyle"
        BasedOn="{StaticResource RadButtonStyle}"
        TargetType="telerik:RadButton">
        <Setter Property="Padding" Value="0" />
        <Setter Property="Margin" Value="4,0,0,0" />
        <Setter Property="MinHeight" Value="24" />
        <Setter Property="MinWidth" Value="24" />
        <Setter Property="SnapsToDevicePixels" Value="True" />
        <Setter Property="VerticalAlignment" Value="Center" />
        <Setter Property="IsTabStop" Value="False" />
