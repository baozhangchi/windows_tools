                                VerticalScrollBarVisibility="{Binding (ScrollViewer.VerticalScrollBarVisibility), RelativeSource={RelativeSource TemplatedParent}}">
                                <ItemsPresenter />
                            </ScrollViewer>
                        </Border>
                        <Border
                            x:Name="MaximizeAreaTop"
                            Grid.Row="0"
                            Grid.ColumnSpan="2"
                            Background="{telerik1:CrystalResource ResourceKey=AccentPressedBrush}"
                            Opacity="0.1"
                            Visibility="Collapsed" />
                        <Border
                            x:Name="MaximizeAreaBottom"
                            Grid.Row="1"
                            Grid.ColumnSpan="2"
                            Background="{telerik1:CrystalResource ResourceKey=AccentPressedBrush}"
                            Opacity="0.1"
                            Visibility="Collapsed" />
                        <Border
                            x:Name="MaximizeAreaLeft"
                            Grid.RowSpan="2"
                            Grid.Column="0"
                            Background="{telerik1:CrystalResource ResourceKey=AccentPressedBrush}"
                            Opacity="0.1"
                            Visibility="Collapsed" />
                        <Border
