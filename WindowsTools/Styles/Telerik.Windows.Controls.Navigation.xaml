        <Setter Property="helpers:ThemeHelper.CheckedBrush" Value="{telerik:CrystalResource ResourceKey=AccentPressedBrush}" />
        <Setter Property="helpers:ThemeHelper.PressedBrush" Value="{telerik:CrystalResource ResourceKey=AccentPressedBrush}" />
        <Setter Property="Foreground" Value="{telerik:CrystalResource ResourceKey=MarkerBrush}" />
        <Setter Property="HorizontalContentAlignment" Value="Left" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
        <Setter Property="MinHeight" Value="36" />
        <Setter Property="Padding" Value="10,0" />
        <Setter Property="FocusVisualStyle" Value="{x:Null}" />
        <Setter Property="ExpandedIcon" Value="{StaticResource GlyphArrowChevronUp}" />
        <Setter Property="ExpandedIconTemplate">
            <Setter.Value>
                <DataTemplate>
                    <telerik1:RadGlyph Glyph="{Binding}" />
                </DataTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="CollapsedIcon" Value="{StaticResource GlyphArrowChevronDown}" />
        <Setter Property="CollapsedIconTemplate">
            <Setter.Value>
                <DataTemplate>
                    <telerik1:RadGlyph Glyph="{Binding}" />
                </DataTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Template" Value="{StaticResource RadNavigationViewItemTemplate}" />
    </Style>
    <Style BasedOn="{StaticResource RadNavigationViewStyle}" TargetType="telerikNavigation:RadNavigationView" />
    <Style BasedOn="{StaticResource RadNavigationViewItemStyle}" TargetType="telerikNavigation:RadNavigationViewItem" />
    <Style
        x:Key="AddItemsButtonStyle"
        BasedOn="{StaticResource RadButtonStyle}"
        TargetType="telerik:RadButton">
        <Setter Property="mat:MaterialAssist.IsShadowDisabled" Value="True" />
        <Setter Property="mat:MaterialAssist.FocusBrush" Value="Transparent" />
        <Setter Property="CornerRadius" Value="0" />
        <Setter Property="MinWidth" Value="34" />
        <Setter Property="MinHeight" Value="34" />
        <Setter Property="Foreground" Value="{telerik1:CrystalResource ResourceKey=IconBrush}" />
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="BorderBrush" Value="{telerik1:CrystalResource ResourceKey=BasicBrush}" />
        <Setter Property="BorderThickness" Value="1,0" />
        <Setter Property="Padding" Value="0" />
    </Style>
    <Style
        x:Key="TabbedWindowScrollButtonStyle"
        BasedOn="{StaticResource RepeatButtonStyle}"
        TargetType="RepeatButton">
        <Setter Property="mat:MaterialAssist.IsShadowDisabled" Value="True" />
        <Setter Property="mat:MaterialAssist.FocusBrush" Value="Transparent" />
        <Setter Property="mat:MaterialAssist.CornerRadius" Value="0" />
        <Setter Property="MinWidth" Value="34" />
        <Setter Property="MinHeight" Value="34" />
        <Setter Property="Foreground" Value="{telerik1:CrystalResource ResourceKey=IconBrush}" />
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="BorderBrush" Value="{telerik1:CrystalResource ResourceKey=BasicBrush}" />
        <Setter Property="BorderThickness" Value="1,0" />
        <Setter Property="Padding" Value="0" />
        <Setter Property="IsTabStop" Value="False" />
        <Style.Triggers>
            <Trigger Property="IsEnabled" Value="False">
                <Setter Property="Background" Value="Transparent" />
            </Trigger>
        </Style.Triggers>
    </Style>
    <Style
        x:Key="RadTabbedWindowTabItemStyle"
        BasedOn="{StaticResource RadTabItemStyle}"
        TargetType="telerikNavigation:RadTabItem">
        <Setter Property="telerik:WindowHost.HitTestable" Value="True" />
        <Setter Property="HorizontalContentAlignment" Value="Left" />
        <Setter Property="CloseButtonVisibility" Value="Visible" />
        <Setter Property="mat:MaterialAssist.MouseOverBrush" Value="{telerik1:CrystalResource ResourceKey=MainBrush}" />
        <Setter Property="mat:MaterialAssist.CheckedBrush" Value="{telerik1:CrystalResource ResourceKey=AlternativeBrush}" />
        <Setter Property="MinHeight" Value="34" />
        <Setter Property="BorderBrush" Value="{telerik1:CrystalResource ResourceKey=BasicBrush}" />
        <Setter Property="BorderThickness" Value="1" />

        <!--  Margin should be same value as border thickness to avoid double borders  -->
        <Setter Property="Margin" Value="0,0,-1,0" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikNavigation:RadTabItem">

                    <!--  x:Name is used in code  -->
                    <Grid x:Name="wrapper">
                        <Border
                            x:Name="BorderVisual"
                            Background="{TemplateBinding Background}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness,
                                                              Converter={StaticResource ThicknessToOrientedThicknessConverter},
                                                              ConverterParameter=LeftRightBottom}" />
                        <telerikPrimitives:LayoutTransformControl x:Name="OrientationTransform">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>
                                <ContentPresenter
                                    x:Name="HeaderElement"
                                    Margin="{TemplateBinding Padding}"
                                    HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                                    Content="{TemplateBinding Header}"
                                    ContentStringFormat="{TemplateBinding HeaderStringFormat}"
                                    ContentTemplate="{TemplateBinding HeaderTemplate}"
                                    ContentTemplateSelector="{TemplateBinding HeaderTemplateSelector}"
                                    RecognizesAccessKey="True"
                                    SnapsToDevicePixels="True"
                                    TextBlock.Foreground="{TemplateBinding HeaderForeground}" />
                                <telerik:RadButton
                                    x:Name="PinButton"
                                    Grid.Column="1"
                                    Command="telerikTabControl:TabItemCommands.TogglePin"
                                    IsTabStop="False"
                                    Style="{StaticResource PinCloseButtonStyle}"
                                    Visibility="{TemplateBinding PinButtonVisibility}">
                                    <telerik:RadGlyph
                                        x:Name="PinGlyph"
                                        Glyph="{StaticResource GlyphUnpin}"
                                        IsHitTestVisible="False" />
                                </telerik:RadButton>
                                <telerik:RadButton
                                    x:Name="CloseButton"
                                    Grid.Column="2"
                                    Command="telerikTabControl:TabItemCommands.Close"
                                    IsTabStop="False"
                                    Style="{StaticResource CloseButtonStyle}"
                                    Visibility="{TemplateBinding CloseButtonVisibility}">
                                    <telerik:RadGlyph
                                        x:Name="CloseGlyph"
                                        FontSize="12"
                                        Glyph="{StaticResource GlyphClose}"
                                        IsHitTestVisible="False" />
                                </telerik:RadButton>
                            </Grid>
                        </telerikPrimitives:LayoutTransformControl>
                    </Grid>
                    <ControlTemplate.Triggers>

                        <!--  Selected  -->
                        <Trigger Property="IsSelected" Value="True">
                            <Setter TargetName="BorderVisual" Property="Background" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(mat:MaterialAssist.CheckedBrush), Mode=OneWay}" />
                            <Setter TargetName="BorderVisual" Property="BorderThickness" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=BorderThickness, Converter={StaticResource ThicknessToOrientedThicknessConverter}, ConverterParameter=LeftRight}" />
                        </Trigger>
                        <Trigger SourceName="wrapper" Property="IsMouseOver" Value="True">
                            <Setter TargetName="BorderVisual" Property="Background" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(mat:MaterialAssist.MouseOverBrush), Mode=OneWay}" />
                        </Trigger>
                        <Trigger Property="IsPinned" Value="True">
                            <Setter TargetName="PinGlyph" Property="Glyph" Value="{StaticResource GlyphPin}" />
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="BorderVisual" Property="Background" Value="{telerik1:CrystalResource ResourceKey=AlternativeBrush}" />
                            <Setter TargetName="HeaderElement" Property="TextBlock.Foreground" Value="{telerik1:CrystalResource ResourceKey=MarkerBrush}" />
                            <Setter TargetName="PinButton" Property="Foreground" Value="{telerik1:CrystalResource ResourceKey=IconBrush}" />
                            <Setter TargetName="CloseButton" Property="Foreground" Value="{telerik1:CrystalResource ResourceKey=IconBrush}" />
                            <Setter TargetName="HeaderElement" Property="Opacity" Value="{telerik1:CrystalResource ResourceKey=DisabledOpacity}" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <ControlTemplate x:Key="TabbedWindowTabControlTemplate" TargetType="tabbedWindow:TabbedWindowTabControl">
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto" MinHeight="36" />
                <RowDefinition Height="*" />
            </Grid.RowDefinitions>
            <Grid x:Name="PART_HeaderGrid" SnapsToDevicePixels="True">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="Auto" />
                </Grid.ColumnDefinitions>
                <Border
                    x:Name="HighlightBorder"
                    Grid.ColumnSpan="3"
                    Height="8"
                    VerticalAlignment="Top"
                    Panel.ZIndex="1"
                    BorderBrush="{telerik1:CrystalResource ResourceKey=PrimaryBackgroundBrush}"
                    BorderThickness="2,2,2,0"
                    CornerRadius="{TemplateBinding mat:MaterialAssist.CornerRadius,
                                                   Converter={StaticResource CornerRadiusConverter},
                                                   ConverterParameter=TopLeftRight}"
                    Opacity="0.5" />
                <Border
                    x:Name="HeaderBackground"
                    Grid.ColumnSpan="3"
                    Background="{TemplateBinding HeaderBackground}"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    BorderThickness="0,0,0,1"
                    CornerRadius="{TemplateBinding mat:MaterialAssist.CornerRadius,
                                                   Converter={StaticResource CornerRadiusConverter},
                                                   ConverterParameter=TopLeftRight}"
                    Visibility="{TemplateBinding BackgroundVisibility}" />
                <telerikPrimitives:LayoutTransformControl
                    x:Name="AdditionalContentTransform"
                    Grid.Column="2"
                    Margin="8,0,0,0"
                    VerticalAlignment="Center">
                    <ContentPresenter x:Name="AdditionalContentElement" Content="{TemplateBinding AdditionalContent}" />
                </telerikPrimitives:LayoutTransformControl>
                <telerikPrimitives:LayoutTransformControl x:Name="AdditionalLeftContentTransform" VerticalAlignment="Center">
                    <ContentPresenter x:Name="AdditionalLeftContentElement" Content="{TemplateBinding AdditionalLeftContent}" />
                </telerikPrimitives:LayoutTransformControl>
                <panels:RadTabbedWindowPanel
                    x:Name="PART_TabbedWindowPanel"
                    Grid.Column="1"
                    Margin="2,0"
                    HorizontalAlignment="Left">
                    <RepeatButton
                        x:Name="LeftScrollButtonElement"
                        AutomationProperties.Name="scroll left"
                        Style="{StaticResource TabbedWindowScrollButtonStyle}">
                        <telerik:RadGlyph Glyph="{StaticResource GlyphArrowChevronLeft}" />
                    </RepeatButton>
                    <ScrollViewer
                        x:Name="ScrollViewerElement"
                        BorderThickness="0"
                        HorizontalScrollBarVisibility="{Binding (ScrollViewer.HorizontalScrollBarVisibility), RelativeSource={RelativeSource TemplatedParent}}"
                        IsTabStop="False"
                        Style="{StaticResource EmptyScrollViewerStyle}"
                        VerticalScrollBarVisibility="Disabled">
                        <ItemsPresenter />
                    </ScrollViewer>
                    <RepeatButton
                        x:Name="RightScrollButtonElement"
                        Margin="-1,0,0,0"
                        telerik:RadDockPanel.Dock="Right"
                        AutomationProperties.Name="scroll right"
                        Style="{StaticResource TabbedWindowScrollButtonStyle}">
                        <telerik:RadGlyph Glyph="{StaticResource GlyphArrowChevronRight}" />
                    </RepeatButton>
                    <telerik:RadButton
                        x:Name="PART_AddItemsButton"
                        Margin="-1,0,0,0"
                        Style="{TemplateBinding AddButtonStyle}"
                        Visibility="{TemplateBinding AddButtonVisibility}">
                        <telerik:RadGlyph x:Name="PlusGlyph" Glyph="{StaticResource GlyphPlus}" />
                    </telerik:RadButton>
                </panels:RadTabbedWindowPanel>
                <Canvas
                    x:Name="DropIndicatorPanel"
                    Grid.Column="1"
                    HorizontalAlignment="Left"
                    Visibility="Collapsed">
                    <Border
                        x:Name="DropIndicator"
                        VerticalAlignment="Center"
                        Background="{telerik1:CrystalResource ResourceKey=AccentPressedBrush}"
                        BorderBrush="{telerik1:CrystalResource ResourceKey=AccentPressedBrush}"
                        IsHitTestVisible="False"
                        Opacity="0.2" />
                </Canvas>
            </Grid>
            <Border
                x:Name="ContentBorder"
                Grid.Row="1"
                Padding="{TemplateBinding Padding}"
                Background="{TemplateBinding Background}"
                BorderBrush="{TemplateBinding BorderBrush}"
                BorderThickness="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=BorderThickness, Mode=TwoWay}"
                CornerRadius="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(mat:MaterialAssist.CornerRadius), Converter={StaticResource CornerRadiusConverter}, ConverterParameter=BottomLeftRight}">
                <Grid telerik:CornerRadiusHelper.ClipRadius="{Binding ElementName=ContentBorder, Path=CornerRadius}" telerik:CornerRadiusHelper.ClipRadiusOffset="{TemplateBinding telerik:CornerRadiusHelper.ClipRadiusOffset}">
                    <ContentPresenter
                        x:Name="ContentElement"
                        ContentTemplate="{TemplateBinding SelectedContentTemplate}"
                        ContentTemplateSelector="{TemplateBinding SelectedContentTemplateSelector}"
                        Visibility="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsContentPreserved, Converter={StaticResource InvertedBooleanToVisibilityConverter}}" />
                    <Grid x:Name="ContentElementsPanel" Visibility="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsContentPreserved, Converter={StaticResource BooleanToVisibilityConverter}}" />
                </Grid>
            </Border>
        </Grid>
    </ControlTemplate>
    <Style
        x:Key="TabbedWindowTabControlStyle"
        BasedOn="{StaticResource RadTabControlStyle}"
        TargetType="tabbedWindow:TabbedWindowTabControl">
        <Setter Property="CloseTabsOnMouseMiddleButtonDown" Value="True" />
        <Setter Property="Template" Value="{StaticResource TabbedWindowTabControlTemplate}" />
        <Setter Property="ScrollViewer.HorizontalScrollBarVisibility" Value="Auto" />
        <Setter Property="AddButtonStyle" Value="{StaticResource AddItemsButtonStyle}" />
        <Setter Property="HeaderBackground" Value="{telerik1:CrystalResource ResourceKey=HeaderBrush}" />
        <Setter Property="Background" Value="{telerik1:CrystalResource ResourceKey=AlternativeBrush}" />
        <Setter Property="BorderBrush" Value="{telerik1:CrystalResource ResourceKey=ComplementaryBasicBrush}" />
        <Setter Property="BorderThickness" Value="0" />
        <Setter Property="ItemContainerStyle" Value="{DynamicResource RadTabbedWindowTabItemStyle}" />
        <Setter Property="telerik:CornerRadiusHelper.ClipRadiusOffset" Value="-1" />
        <Setter Property="ItemsPanel">
            <Setter.Value>
                <ItemsPanelTemplate>
                    <telerikNavigationPrimitives:RadBrowserPanel ItemMinWidth="{Binding ItemMinWidth, FallbackValue=80, RelativeSource={RelativeSource AncestorType={x:Type tabbedWindow:TabbedWindowTabControl}}}" ItemWidth="{Binding ItemWidth, FallbackValue=120, RelativeSource={RelativeSource AncestorType={x:Type tabbedWindow:TabbedWindowTabControl}}}" />
                </ItemsPanelTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style BasedOn="{StaticResource TabbedWindowTabControlStyle}" TargetType="tabbedWindow:TabbedWindowTabControl" />
    <ControlTemplate x:Key="RadTabbedWindowTemplate" TargetType="telerikNavigation:RadTabbedWindow">
        <Grid x:Name="LayoutRoot" Margin="10,8">
            <mat:Shadow
                Background="{TemplateBinding Background}"
                CornerRadius="{TemplateBinding CornerRadius}"
                ShadowDepth="{TemplateBinding mat:MaterialAssist.ShadowDepth}" />
            <Border
                x:Name="WindowOuterBorder"
                BorderBrush="{TemplateBinding BorderBrush}"
                BorderThickness="{TemplateBinding BorderThickness}"
                CornerRadius="{TemplateBinding CornerRadius}">
                <AdornerDecorator>
                    <Border
                        x:Name="Header"
                        Margin="{TemplateBinding Padding}"
                        Background="{TemplateBinding BorderBackground}"
                        CornerRadius="{Binding CornerRadius, ElementName=WindowOuterBorder}">
                        <tabbedWindow:TabbedWindowTabControl
                            x:Name="PART_TabControl"
                            mat:MaterialAssist.CornerRadius="{TemplateBinding CornerRadius}"
                            AddButtonStyle="{TemplateBinding AddButtonStyle}"
                            AddButtonVisibility="{TemplateBinding AddButtonVisibility}"
                            ContentTemplate="{TemplateBinding ContentTemplate}"
                            ContentTemplateSelector="{TemplateBinding ContentTemplateSelector}"
                            DisplayMemberPath="{TemplateBinding DisplayMemberPath}"
                            IsContentPreserved="{TemplateBinding IsContentPreserved}"
                            ItemContainerStyle="{TemplateBinding ItemContainerStyle}"
                            ItemContainerStyleSelector="{TemplateBinding ItemContainerStyleSelector}"
                            ItemMinWidth="{TemplateBinding ItemMinWidth}"
                            ItemTemplate="{TemplateBinding ItemTemplate}"
                            ItemTemplateSelector="{TemplateBinding ItemTemplateSelector}"
                            ItemWidth="{TemplateBinding ItemWidth}"
                            ItemsPanel="{TemplateBinding ItemsPanel}"
                            ItemsSource="{TemplateBinding ItemsSource}"
                            ScrollMode="{TemplateBinding ScrollMode}"
                            SelectedIndex="{Binding SelectedIndex, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}"
                            SelectedItem="{Binding SelectedItem, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}"
                            SelectedItemRemoveBehaviour="{TemplateBinding SelectedItemRemoveBehaviour}">
                            <tabbedWindow:TabbedWindowTabControl.AdditionalLeftContent>
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto" />
                                        <ColumnDefinition Width="*" />
                                    </Grid.ColumnDefinitions>
                                    <ContentPresenter
                                        x:Name="IconContent"
                                        Grid.Column="0"
                                        Margin="8,2,0,0"
                                        HorizontalAlignment="Left"
                                        VerticalAlignment="Center"
                                        Content="{TemplateBinding Icon}"
                                        ContentTemplate="{TemplateBinding IconTemplate}" />
                                    <ContentPresenter
                                        x:Name="HeaderContent"
                                        Grid.Column="1"
                                        Margin="8,2,8,0"
                                        HorizontalAlignment="Stretch"
                                        VerticalAlignment="Center"
                                        Content="{TemplateBinding Header}"
                                        ContentTemplate="{TemplateBinding HeaderTemplate}"
                                        ContentTemplateSelector="{TemplateBinding HeaderTemplateSelector}"
                                        SnapsToDevicePixels="True"
                                        TextBlock.FontFamily="{TemplateBinding FontFamily}"
                                        TextBlock.FontSize="{TemplateBinding FontSize}"
                                        TextBlock.Foreground="{TemplateBinding Foreground}" />
                                </Grid>
                            </tabbedWindow:TabbedWindowTabControl.AdditionalLeftContent>
                            <tabbedWindow:TabbedWindowTabControl.AdditionalContent>
                                <Border x:Name="PART_HeaderButtonsBorder" VerticalAlignment="Top">
                                    <StackPanel x:Name="HeaderButtons" Orientation="Horizontal">
                                        <telerik:RadButton
                                            x:Name="PART_MinimizeButton"
                                            Margin="8,0,4,0"
                                            Command="{x:Static telerik:WindowCommands.Minimize}"
                                            Style="{StaticResource WindowMinimizeButtonStyle}"
                                            Visibility="{Binding IsEnabled, RelativeSource={RelativeSource Self}, Converter={StaticResource BooleanToVisibilityConverter}}">
                                            <ToolTipService.ToolTip>
                                                <ToolTip telerik:LocalizationManager.ResourceKey="Minimize" />
                                            </ToolTipService.ToolTip>
                                            <telerik:RadGlyph
                                                HorizontalAlignment="Center"
                                                VerticalAlignment="Center"
                                                FontSize="10"
                                                Glyph="{StaticResource GlyphMinimize}" />
                                        </telerik:RadButton>
                                        <telerik:RadButton
                                            x:Name="PART_RestoreButton"
                                            Margin="4,0"
                                            Command="{x:Static telerik:WindowCommands.Restore}"
                                            Style="{StaticResource WindowMaximizeButtonStyle}"
                                            Visibility="{Binding IsEnabled, RelativeSource={RelativeSource Self}, Converter={StaticResource BooleanToVisibilityConverter}}">
                                            <ToolTipService.ToolTip>
                                                <ToolTip telerik:LocalizationManager.ResourceKey="Restore" />
                                            </ToolTipService.ToolTip>
                                            <telerik:RadGlyph
                                                HorizontalAlignment="Center"
                                                VerticalAlignment="Center"
                                                FontSize="10"
                                                Glyph="{StaticResource GlyphWindowCollapse}" />
                                        </telerik:RadButton>
                                        <telerik:RadButton
                                            x:Name="PART_MaximizeButton"
                                            Margin="4,0"
                                            Command="{x:Static telerik:WindowCommands.Maximize}"
                                            Style="{StaticResource WindowMaximizeButtonStyle}"
                                            Visibility="{Binding IsEnabled, RelativeSource={RelativeSource Self}, Converter={StaticResource BooleanToVisibilityConverter}}">
                                            <ToolTipService.ToolTip>
                                                <ToolTip telerik:LocalizationManager.ResourceKey="Maximize" />
                                            </ToolTipService.ToolTip>
                                            <telerik:RadGlyph
                                                HorizontalAlignment="Center"
                                                VerticalAlignment="Center"
                                                FontSize="10"
                                                Glyph="{StaticResource GlyphWindow}" />
                                        </telerik:RadButton>
                                        <telerik:RadButton
                                            x:Name="PART_CloseButton"
                                            Margin="4,0,8,0"
                                            Command="{x:Static telerik:WindowCommands.Close}"
                                            Style="{StaticResource WindowCloseButtonStyle}"
                                            Visibility="{Binding IsEnabled, RelativeSource={RelativeSource Self}, Converter={StaticResource BooleanToVisibilityConverter}}">
                                            <ToolTipService.ToolTip>
                                                <ToolTip telerik:LocalizationManager.ResourceKey="Close" />
                                            </ToolTipService.ToolTip>
                                            <telerik:RadGlyph
                                                HorizontalAlignment="Center"
                                                VerticalAlignment="Center"
                                                FontSize="10"
                                                Glyph="{StaticResource GlyphClose}" />
                                        </telerik:RadButton>
                                    </StackPanel>
                                </Border>
                            </tabbedWindow:TabbedWindowTabControl.AdditionalContent>
                        </tabbedWindow:TabbedWindowTabControl>
                    </Border>
                </AdornerDecorator>
            </Border>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="WindowState" Value="Maximized">
                <Setter TargetName="WindowOuterBorder" Property="BorderThickness" Value="0" />
                <Setter TargetName="LayoutRoot" Property="Margin" Value="0" />
                <Setter TargetName="Header" Property="Margin" Value="0" />
                <Setter TargetName="PART_TabControl" Property="mat:MaterialAssist.CornerRadius" Value="0" />
                <Setter TargetName="WindowOuterBorder" Property="CornerRadius" Value="0" />
            </Trigger>
            <Trigger Property="WindowState" Value="Minimized">
                <Setter TargetName="LayoutRoot" Property="HorizontalAlignment" Value="Left" />
                <Setter TargetName="LayoutRoot" Property="VerticalAlignment" Value="Top" />
                <Setter TargetName="LayoutRoot" Property="Margin" Value="0" />
            </Trigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition SourceName="HeaderContent" Property="Content" Value="{x:Null}" />
                    <Condition SourceName="HeaderContent" Property="ContentTemplate" Value="{x:Null}" />
                    <Condition SourceName="HeaderContent" Property="ContentTemplateSelector" Value="{x:Null}" />
                </MultiTrigger.Conditions>
                <Setter TargetName="HeaderContent" Property="Visibility" Value="Collapsed" />
            </MultiTrigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition SourceName="IconContent" Property="Content" Value="{x:Null}" />
                    <Condition SourceName="IconContent" Property="ContentTemplate" Value="{x:Null}" />
                </MultiTrigger.Conditions>
                <Setter TargetName="IconContent" Property="Visibility" Value="Collapsed" />
            </MultiTrigger>
            <Trigger Property="mat:MaterialAssist.IsShadowDisabled" Value="True">
                <Setter TargetName="LayoutRoot" Property="Margin" Value="0" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <Style
        x:Key="RadTabbedWindowStyle"
        BasedOn="{StaticResource RadWindowStyle}"
        TargetType="telerikNavigation:RadTabbedWindow">
        <Setter Property="mat:MaterialAssist.ShadowDepth" Value="{StaticResource PopupShadowDepth}" />
        <Setter Property="CornerRadius" Value="{telerik1:CrystalResource ResourceKey=CornerRadius}" />
        <Setter Property="Template" Value="{StaticResource RadTabbedWindowTemplate}" />
        <Setter Property="AddButtonStyle" Value="{StaticResource AddItemsButtonStyle}" />
        <Setter Property="Foreground" Value="{telerik1:CrystalResource ResourceKey=MarkerBrush}" />
        <Setter Property="FontFamily" Value="{telerik1:CrystalResource ResourceKey=FontFamily}" />
        <Setter Property="FontSize" Value="{telerik1:CrystalResource ResourceKey=FontSize}" />
        <Setter Property="Header" Value="{x:Null}" />
        <Setter Property="BorderBackground" Value="Transparent" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="Padding" Value="0" />
        <Setter Property="MinHeight" Value="48" />
        <Setter Property="MinWidth" Value="200" />
        <Setter Property="ItemContainerStyle" Value="{StaticResource RadTabbedWindowTabItemStyle}" />
        <Setter Property="ItemsPanel">
            <Setter.Value>
                <ItemsPanelTemplate>
                    <telerikNavigationPrimitives:RadBrowserPanel ItemMinWidth="{Binding ItemMinWidth, FallbackValue=80, RelativeSource={RelativeSource AncestorType={x:Type telerikNavigation:RadTabbedWindow}}}" ItemWidth="{Binding ItemWidth, FallbackValue=120, RelativeSource={RelativeSource AncestorType={x:Type telerikNavigation:RadTabbedWindow}}}" />
                </ItemsPanelTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="navigation:RadWindowInteropHelper.ShowInTaskbar" Value="True" />
        <Style.Triggers>
            <Trigger Property="navigation:RadWindowInteropHelper.AllowTransparency" Value="False">
                <Setter Property="navigation:RadWindowInteropHelper.ClipMaskCornerRadius" Value="0" />
                <Setter Property="animation:AnimationManager.IsAnimationEnabled" Value="False" />
                <Setter Property="mat:MaterialAssist.IsShadowDisabled" Value="True" />
            </Trigger>
        </Style.Triggers>
    </Style>
    <Style BasedOn="{StaticResource RadTabbedWindowStyle}" TargetType="telerikNavigation:RadTabbedWindow" />
    <ControlTemplate x:Key="RadSplashScreenTemplate" TargetType="telerikNavigation:RadSplashScreen">
        <Grid x:Name="LayoutRootGrid">
            <mat:Shadow
                x:Name="Shadow"
                Background="{TemplateBinding Background}"
                BorderThickness="{TemplateBinding BorderThickness}"
                CornerRadius="{TemplateBinding helpers:ThemeHelper.CornerRadius}"
                ShadowDepth="{TemplateBinding mat:MaterialAssist.ShadowDepth}" />
            <Border
                x:Name="BackgroundVisual"
                Background="{TemplateBinding Background}"
                BorderBrush="{TemplateBinding BorderBrush}"
                BorderThickness="{TemplateBinding BorderThickness}"
                CornerRadius="{TemplateBinding helpers:ThemeHelper.CornerRadius}" />
            <Grid x:Name="ContentGrid" Margin="{TemplateBinding Padding}">
                <Grid.RowDefinitions>
                    <RowDefinition Height="*" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                </Grid.RowDefinitions>
                <Image
                    Width="{TemplateBinding ImageWidth}"
                    Height="{TemplateBinding ImageHeight}"
                    Source="{Binding Path=ImagePath, RelativeSource={RelativeSource TemplatedParent}, TargetNullValue={x:Null}}"
                    Stretch="{TemplateBinding ImageStretch}"
                    Visibility="{Binding Path=ImagePath, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource NullToVisibilityConverter}}" />
                <ContentPresenter
                    Grid.Row="1"
                    Margin="0,20,0,0"
                    HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                    ContentSource="Content"
                    TextBlock.Foreground="{TemplateBinding Foreground}" />
                <telerik1:RadProgressBar
                    x:Name="LoadingProgressBar"
                    Grid.Row="2"
                    Margin="0,20"
                    IsIndeterminate="{TemplateBinding IsIndeterminate}"
                    Maximum="{TemplateBinding MaxValue}"
                    Minimum="{TemplateBinding MinValue}"
                    Visibility="{TemplateBinding ProgressBarVisibility}"
                    Value="{TemplateBinding ProgressValue}" />
                <ContentPresenter
                    Grid.Row="3"
                    Margin="0,20,0,0"
                    HorizontalAlignment="{TemplateBinding HorizontalFooterAlignment}"
                    ContentSource="Footer"
                    TextBlock.Foreground="{TemplateBinding Foreground}" />
            </Grid>
        </Grid>
    </ControlTemplate>
    <Style x:Key="RadSplashScreenStyle" TargetType="telerikNavigation:RadSplashScreen">
        <Setter Property="helpers:ThemeHelper.CornerRadius" Value="{telerik1:CrystalResource ResourceKey=CornerRadius}" />
        <Setter Property="mat:MaterialAssist.ShadowDepth" Value="Depth2" />
        <Setter Property="FontFamily" Value="{telerik1:CrystalResource ResourceKey=FontFamily}" />
        <Setter Property="FontSize" Value="{telerik1:CrystalResource ResourceKey=FontSize}" />
        <Setter Property="Foreground" Value="{telerik:CrystalResource ResourceKey=MarkerBrush}" />
        <Setter Property="Background" Value="{telerik:CrystalResource ResourceKey=AlternativeBrush}" />
        <Setter Property="BorderBrush" Value="{telerik:CrystalResource ResourceKey=ComplementaryBasicBrush}" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="Padding" Value="20,20,20,10" />
        <Setter Property="FocusVisualStyle" Value="{x:Null}" />
        <Setter Property="Template" Value="{StaticResource RadSplashScreenTemplate}" />
    </Style>
    <Style BasedOn="{StaticResource RadSplashScreenStyle}" TargetType="telerikNavigation:RadSplashScreen" />
    <Style
        x:Key="KeyButtonStyle"
        BasedOn="{StaticResource RadButtonStyle}"
        TargetType="telerik1:RadButton">
        <Setter Property="Padding" Value="0" />
        <Setter Property="FontSize" Value="{Binding FontSize, RelativeSource={RelativeSource AncestorType={x:Type telerikNavigation:RadVirtualKeyboard}}}" />
        <Setter Property="Focusable" Value="False" />
    </Style>
    <ControlTemplate x:Key="RadVirtualKeyboardTemplate" TargetType="telerikNavigation:RadVirtualKeyboard">
        <Grid>
            <mat:Shadow
                x:Name="Shadow"
                Background="{TemplateBinding Background}"
                BorderThickness="{TemplateBinding BorderThickness}"
                ShadowDepth="{TemplateBinding mat:MaterialAssist.ShadowDepth}" />
            <Border
                Background="{TemplateBinding Background}"
                BorderBrush="{TemplateBinding BorderBrush}"
                BorderThickness="{TemplateBinding BorderThickness}"
                SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}">
                <Grid x:Name="PART_ContentElement" Margin="2" />
            </Border>
        </Grid>
    </ControlTemplate>
    <DataTemplate x:Key="RegularKeyTemplate">
        <telerik1:RadButton
            HorizontalContentAlignment="Stretch"
            VerticalContentAlignment="Stretch"
            AutomationProperties.AutomationId="{Binding DisplayText}"
            Command="{Binding KeyCommand}"
            Style="{StaticResource KeyButtonStyle}">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition />
                    <RowDefinition />
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition />
                    <ColumnDefinition />
                </Grid.ColumnDefinitions>
                <TextBlock
                    Grid.Row="0"
                    Grid.Column="0"
                    Margin="3,0,0,0"
                    Text="{Binding ShiftText}"
                    Visibility="{Binding ShowSecondaryText, Converter={StaticResource BooleanToVisibilityConverter}}" />
                <TextBlock
                    Grid.RowSpan="2"
                    Grid.ColumnSpan="2"
                    HorizontalAlignment="Center"
                    VerticalAlignment="Center"
                    Text="{Binding DisplayText}" />
            </Grid>
        </telerik1:RadButton>
    </DataTemplate>
    <DataTemplate x:Key="SpecialKeyTemplate">
        <telerik1:RadButton
            AutomationProperties.AutomationId="{Binding DisplayText}"
            Command="{Binding KeyCommand}"
            Content="{Binding DisplayText}"
            Style="{StaticResource KeyButtonStyle}" />
    </DataTemplate>
    <DataTemplate x:Key="NumpadKeyTemplate">
        <telerik1:RadButton
            AutomationProperties.AutomationId="{Binding DisplayText}"
            Command="{Binding KeyCommand}"
            Content="{Binding DisplayText}"
            Style="{StaticResource KeyButtonStyle}" />
    </DataTemplate>
    <DataTemplate x:Key="LockKeyTemplate">
        <telerik1:RadToggleButton
            Padding="0"
            AutomationProperties.AutomationId="{Binding DisplayText}"
            Command="{Binding KeyCommand}"
            Content="{Binding DisplayText}"
            Focusable="False"
            FontSize="{Binding FontSize, RelativeSource={RelativeSource AncestorType={x:Type telerikNavigation:RadVirtualKeyboard}}}"
            IsChecked="{Binding IsChecked}" />
    </DataTemplate>
    <telerikNavigation:VirtualKeyboardTemplateSelector
        x:Key="VirtualKeyboardTemplateSelector"
        LockTemplate="{StaticResource LockKeyTemplate}"
        NumpadTemplate="{StaticResource NumpadKeyTemplate}"
        RegularTemplate="{StaticResource RegularKeyTemplate}"
        SpecialTemplate="{StaticResource SpecialKeyTemplate}" />
    <Style x:Key="RadVirtualKeyboardStyle" TargetType="telerikNavigation:RadVirtualKeyboard">
        <Setter Property="Template" Value="{StaticResource RadVirtualKeyboardTemplate}" />
        <Setter Property="VirtualKeyboardTemplateSelector" Value="{StaticResource VirtualKeyboardTemplateSelector}" />
        <Setter Property="mat:MaterialAssist.ShadowDepth" Value="Depth1" />
        <Setter Property="Background" Value="{telerik:CrystalResource ResourceKey=PrimaryBackgroundBrush}" />
        <Setter Property="BorderBrush" Value="{telerik:CrystalResource ResourceKey=ComplementaryBasicBrush}" />
        <Setter Property="BorderThickness" Value="0" />
        <Setter Property="FontSize" Value="12" />
        <Setter Property="Width" Value="1110" />
        <Setter Property="Height" Value="300" />
        <Setter Property="SnapsToDevicePixels" Value="True" />
        <Setter Property="UseLayoutRounding" Value="True" />
        <Setter Property="FocusVisualStyle" Value="{x:Null}" />
    </Style>
    <Style
        x:Key="RadVirtualKeyboardWindowStyle"
        BasedOn="{StaticResource RadWindowStyle}"
        TargetType="telerikNavigation:RadVirtualKeyboardWindow">
        <Setter Property="Focusable" Value="False" />
        <Setter Property="HideMaximizeButton" Value="True" />
        <Setter Property="IsTopmost" Value="True" />
        <Setter Property="ResizeMode" Value="CanMinimize" />
        <Setter Property="Padding" Value="0" />
        <Setter Property="Header" Value="{x:Null}" />
    </Style>
    <Style BasedOn="{StaticResource RadVirtualKeyboardStyle}" TargetType="telerikNavigation:RadVirtualKeyboard" />
    <Style BasedOn="{StaticResource RadVirtualKeyboardWindowStyle}" TargetType="telerikNavigation:RadVirtualKeyboardWindow" />
</ResourceDictionary>
