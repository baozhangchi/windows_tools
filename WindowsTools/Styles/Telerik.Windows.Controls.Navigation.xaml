                                    CornerRadius="{TemplateBinding mat:MaterialAssist.CornerRadius}"
                                    DockPanel.Dock="Top"
                                    Visibility="{TemplateBinding HeaderVisibility}">
                                    <Grid MinHeight="30">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition />
                                            <ColumnDefinition Width="Auto" />
                                        </Grid.ColumnDefinitions>
                                        <ContentControl
                                            x:Name="TitleElement"
                                            VerticalAlignment="Center"
                                            HorizontalContentAlignment="Stretch"
                                            VerticalContentAlignment="Center"
                                            ContentControl.Foreground="{telerik1:CrystalResource ResourceKey=AccentPressedBrush}"
                                            ContentControl.Margin="8 0 0 0"
                                            ContentTemplate="{TemplateBinding TitleTemplate}"
                                            FontSize="{telerik1:CrystalResource ResourceKey=FontSize}"
                                            FontWeight="SemiBold"
                                            IsTabStop="False" />
                                        <telerik1:RadToggleButton
                                            x:Name="MinimizeButton"
                                            Grid.Column="1"
                                            AutomationProperties.Name="Minimize"
                                            IsChecked="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsMinimized, Mode=TwoWay}"
                                            Style="{TemplateBinding MinimizeButtonStyle}"
                                            Visibility="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsMinimizable, Converter={StaticResource BooleanToVisibilityConverter}}">
                                            <telerik1:RadGlyph
                                                x:Name="arrowGlyph"
                                                Foreground="{telerik1:CrystalResource ResourceKey=IconBrush}"
                                                Glyph="{StaticResource GlyphArrowChevronLeft}" />
                                        </telerik1:RadToggleButton>
                                    </Grid>
                                </Border>
                                <Border
                                    MinHeight="{TemplateBinding MinimizedAreaMinHeight}"
                                    Background="{telerik1:CrystalResource ResourceKey=ComplementaryBrush}"
                                    BorderBrush="{TemplateBinding BorderBrush}"
                                    BorderThickness="0,1,0,0"
                                    DockPanel.Dock="Bottom"
                                    Visibility="{TemplateBinding MinimizedAreaVisibility}">
                                    <DockPanel x:Name="DockPanelVisual">
                                        <ToggleButton
                                            x:Name="DropDownButtonElement"
                                            AutomationProperties.Name="Overflow"
                                            IsChecked="{Binding IsDropDownOpen, Mode=TwoWay, RelativeSource={RelativeSource TemplatedParent}}"
                                            Style="{StaticResource OutlookBarDropDownButtonStyle}">
                                            <ToolTipService.ToolTip>
                                                <TextBlock Text="{telerik1:LocalizableResource Key=OutlookBarConfigureButtons}" />
                                            </ToolTipService.ToolTip>
                                            <telerikNavigation:RadContextMenu.ContextMenu>
                                                <telerikTabControl:DropDownMenu
                                                    x:Name="DropDownMenuElement"
                                                    DisplayMemberPath="{TemplateBinding DropDownDisplayMemberPath}"
                                                    ItemTemplate="{TemplateBinding ItemDropDownContentTemplate}"
                                                    ItemTemplateSelector="{TemplateBinding ItemDropDownContentTemplateSelector}"
                                                    Placement="Bottom"
                                                    StaysOpen="False"
                                                    Style="{TemplateBinding DropDownStyle}" />
                                            </telerikNavigation:RadContextMenu.ContextMenu>
                                        </ToggleButton>
                                        <outlookBarPrimitives:MinimizedOutlookBarArea x:Name="MinimizedAreaControl" />
                                    </DockPanel>
                                </Border>
                                <ItemsPresenter x:Name="ItemsPresenterElement" DockPanel.Dock="Bottom" />
                                <Thumb
                                    x:Name="HorizontalSplitter"
                                    DockPanel.Dock="Bottom"
                                    Style="{TemplateBinding HorizontalSplitterStyle}">
                                    <ToolTipService.ToolTip>
                                        <TextBlock Text="{telerik1:LocalizableResource Key=OutlookBarHorizontalSplitter}" />
                                    </ToolTipService.ToolTip>
                                </Thumb>
                                <Grid Background="{telerik1:CrystalResource ResourceKey=ComplementaryBrush}">
                                    <ContentPresenter
                                        x:Name="ContentElement"
                                        Margin="{TemplateBinding Padding}"
                                        HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                        ContentTemplate="{TemplateBinding SelectedContentTemplate}"
                                        ContentTemplateSelector="{TemplateBinding SelectedContentTemplateSelector}"
                                        Visibility="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsContentPreserved, Converter={StaticResource InvertedBooleanToVisibilityConverter}}" />
                                    <Grid
                                        x:Name="ContentElementsPanel"
                                        Margin="{TemplateBinding Padding}"
                                        Visibility="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsContentPreserved, Converter={StaticResource BooleanToVisibilityConverter}}" />
                                    <telerik1:RadDropDownButton
                                        x:Name="MinimizedContentElement"
                                        Background="{TemplateBinding HeaderBackground}"
                                        Content="{TemplateBinding MinimizedButtonContent}"
                                        ContentTemplate="{TemplateBinding MinimizedButtonContentTemplate}"
                                        Style="{StaticResource OutlookBarMinimizedContentButtonStyle}"
                                        Visibility="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsMinimized, Converter={StaticResource BooleanToVisibilityConverter}}">
                                        <telerik1:RadDropDownButton.DropDownContent>
                                            <Grid>
                                                <ContentPresenter
                                                    x:Name="MinimizedContentPresenter"
                                                    ContentTemplate="{TemplateBinding SelectedContentTemplate}"
                                                    ContentTemplateSelector="{TemplateBinding SelectedContentTemplateSelector}" />
                                                <Thumb
                                                    x:Name="PopupResizer"
                                                    Width="2"
                                                    HorizontalAlignment="Right"
                                                    VerticalAlignment="Stretch"
                                                    Cursor="SizeWE"
                                                    Opacity="0" />
                                            </Grid>
                                        </telerik1:RadDropDownButton.DropDownContent>
                                    </telerik1:RadDropDownButton>
                                </Grid>
                            </DockPanel>
                        </Border>
                        <Thumb x:Name="VerticalResizer" Style="{TemplateBinding VerticalResizerStyle}">
                            <ToolTipService.ToolTip>
                                <TextBlock Text="{telerik1:LocalizableResource Key=OutlookBarVerticalResizer}" />
                            </ToolTipService.ToolTip>
                        </Thumb>
                    </Grid>
                    <ControlTemplate.Triggers>

                        <!--  Minimized  -->
                        <Trigger Property="IsMinimized" Value="True">
                            <Setter TargetName="TitleElement" Property="Opacity" Value="0" />
                            <Setter TargetName="ContentElement" Property="Opacity" Value="0" />
                            <Setter TargetName="arrowGlyph" Property="Glyph" Value="{StaticResource GlyphArrowChevronRight}" />
                            <Setter TargetName="MinimizeButton" Property="Margin" Value="0" />
                            <Setter TargetName="MinimizeButton" Property="BorderThickness" Value="0" />
                            <Setter TargetName="MinimizeButton" Property="CornerRadius" Value="0" />
                            <Setter TargetName="MinimizeButton" Property="VerticalAlignment" Value="Stretch" />
                            <Setter TargetName="MinimizeButton" Property="Width" Value="28" />
                            <Setter TargetName="DropDownButtonElement" Property="Margin" Value="0" />
                            <Setter TargetName="DropDownButtonElement" Property="BorderThickness" Value="0" />
                            <Setter TargetName="DropDownButtonElement" Property="mat:MaterialAssist.CornerRadius" Value="0" />
                            <Setter TargetName="DropDownButtonElement" Property="VerticalAlignment" Value="Stretch" />
                            <Setter TargetName="DropDownButtonElement" Property="Width" Value="{Binding ActualWidth, ElementName=DockPanelVisual}" />
                        </Trigger>

                        <!--  Restored  -->
                        <Trigger Property="IsMinimized" Value="False">
                            <Setter TargetName="TitleElement" Property="Opacity" Value="1" />
                            <Setter TargetName="ContentElement" Property="Opacity" Value="1" />
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="TitleElement" Property="Opacity" Value="{telerik1:CrystalResource ResourceKey=DisabledOpacity}" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="RadOutlookBarItemStyle" TargetType="telerikNavigation:RadOutlookBarItem">
        <Setter Property="FontFamily" Value="{telerik1:CrystalResource ResourceKey=FontFamily}" />
        <Setter Property="FontSize" Value="{telerik1:CrystalResource ResourceKey=FontSize}" />
        <Setter Property="Foreground" Value="{telerik1:CrystalResource ResourceKey=MarkerBrush}" />
        <Setter Property="Background" Value="{telerik1:CrystalResource ResourceKey=MainBrush}" />
        <Setter Property="BorderBrush" Value="{telerik1:CrystalResource ResourceKey=AlternativeBasicBrush}" />
        <Setter Property="BorderThickness" Value="0,1,0,0" />
        <Setter Property="Padding" Value="8,7" />
        <Setter Property="UseLayoutRounding" Value="True" />
        <Setter Property="HorizontalContentAlignment" Value="Left" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
        <Setter Property="IsTabStop" Value="False" />
        <Setter Property="SnapsToDevicePixels" Value="True" />
        <Setter Property="IconMargin" Value="0 0 6 0" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikNavigation:RadOutlookBarItem">
                    <Grid x:Name="wrapper">
                        <Border
                            x:Name="BorderVisual"
                            Background="{TemplateBinding Background}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}"
                            CornerRadius="{TemplateBinding mat:MaterialAssist.CornerRadius}"
                            SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" />
                        <telerik1:RadDockPanel x:Name="DockPanelVisual" Margin="{TemplateBinding Padding}">
                            <Grid x:Name="ImageContainer">
                                <Image
                                    x:Name="Image"
                                    Margin="{TemplateBinding IconMargin}"
                                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                                    telerik1:RadDockPanel.Dock="Left"
                                    Source="{TemplateBinding Icon}"
                                    Stretch="None"
                                    Visibility="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=Icon, Converter={StaticResource NullToVisibilityConverter}}" />
                            </Grid>
                            <ContentControl
                                x:Name="HeaderElement"
                                HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                                Content="{TemplateBinding Header}"
                                ContentTemplate="{TemplateBinding HeaderTemplate}"
                                ContentTemplateSelector="{TemplateBinding HeaderTemplateSelector}"
                                FontSize="{TemplateBinding FontSize}"
                                FontWeight="{TemplateBinding FontWeight}"
                                Foreground="{TemplateBinding Foreground}"
                                IsTabStop="False" />
                        </telerik1:RadDockPanel>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter Property="Background" Value="{telerik1:CrystalResource ResourceKey=MouseOverBrush}" />
                        </Trigger>
                        <Trigger Property="IsSelected" Value="True">
                            <Setter Property="Background" Value="{telerik1:CrystalResource ResourceKey=PressedBrush}" />
                        </Trigger>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="IsSelected" Value="True" />
                                <Condition Property="IsMouseOver" Value="True" />
                            </MultiTrigger.Conditions>
                            <Setter Property="Background" Value="{telerik1:CrystalResource ResourceKey=MouseOverBrush}" />
                        </MultiTrigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="HeaderElement" Property="Opacity" Value="{telerik1:CrystalResource ResourceKey=DisabledOpacity}" />
                            <Setter TargetName="Image" Property="Opacity" Value="{telerik1:CrystalResource ResourceKey=DisabledOpacity}" />
                        </Trigger>
                        <MultiDataTrigger>
                            <MultiDataTrigger.Conditions>
                                <Condition Binding="{Binding IsMinimized, RelativeSource={RelativeSource AncestorType={x:Type telerikNavigation:RadOutlookBar}}}" Value="True" />
                                <Condition Binding="{Binding Visibility, ElementName=Image}" Value="Visible" />
                            </MultiDataTrigger.Conditions>
                            <Setter TargetName="Image" Property="Margin" Value="{Binding IconMargin, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource ThicknessToOrientedThicknessConverter}, ConverterParameter=TopBottom}" />
                            <Setter TargetName="DockPanelVisual" Property="Margin" Value="{Binding Padding, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource ThicknessToOrientedThicknessConverter}, ConverterParameter=TopBottom}" />
                            <Setter TargetName="ImageContainer" Property="Width" Value="{Binding ActualWidth, ElementName=DockPanelVisual}" />
                        </MultiDataTrigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
