            <Grid.RowDefinitions>
                <RowDefinition Height="Auto" />
                <RowDefinition />
            </Grid.RowDefinitions>
            <mat:Shadow
                Background="{TemplateBinding Background}"
                BorderBrush="{TemplateBinding BorderBrush}"
                BorderThickness="{TemplateBinding BorderThickness}"
                ShadowDepth="{TemplateBinding mat:MaterialAssist.ShadowDepth}"
                SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" />
            <Border
                Background="{telerik1:CrystalResource ResourceKey=AlternativeBrush}"
                BorderBrush="{TemplateBinding BorderBrush}"
                BorderThickness="{Binding RelativeSource={RelativeSource AncestorType=telerikNavigation:LayoutControlTabGroup}, Path=BorderThickness, Converter={StaticResource ThicknessToOrientedThicknessConverter}, ConverterParameter=LeftTopRight}">
                <ItemsPresenter />
            </Border>
            <ScrollViewer
                Grid.Row="1"
