        <ControlTemplate.Triggers>
            <DataTrigger Binding="{Binding Source={x:Static telerik:CrystalPalette.Palette}, Path=WindowButtonsAlignment}" Value="Right">
                <Setter TargetName="HeaderPanel" Property="AlignButtonsToLeft" Value="False" />
                <Setter TargetName="PART_MinimizeButton" Property="Margin" Value="0,0,10,0" />
                <Setter TargetName="PART_RestoreButton" Property="Margin" Value="0,0,10,0" />
                <Setter TargetName="PART_MaximizeButton" Property="Margin" Value="0,0,10,0" />
                <Setter TargetName="PART_CloseButton" Property="Margin" Value="0,0,10,0" />
                <Setter TargetName="PART_MinimizeButton" Property="Grid.Column" Value="0" />
                <Setter TargetName="PART_RestoreButton" Property="Grid.Column" Value="1" />
                <Setter TargetName="PART_MaximizeButton" Property="Grid.Column" Value="2" />
                <Setter TargetName="PART_CloseButton" Property="Grid.Column" Value="3" />
            </DataTrigger>
            <Trigger Property="WindowState" Value="Normal">
                <Setter TargetName="LayoutRoot" Property="Margin" Value="10,8" />
            </Trigger>
            <Trigger Property="WindowState" Value="Maximized">
                <Setter TargetName="WindowOuterBorder" Property="BorderThickness" Value="0" />
                <Setter TargetName="WindowOuterBorder" Property="CornerRadius" Value="0" />
                <Setter TargetName="Shadow" Property="CornerRadius" Value="0" />
            </Trigger>
            <Trigger Property="WindowState" Value="Minimized">
                <Setter Property="MinHeight" Value="28" />
                <Setter TargetName="WindowHeaderRow" Property="MinHeight" Value="28" />
                <Setter TargetName="HeaderContent" Property="Margin" Value="5,0" />
                <Setter TargetName="LayoutRoot" Property="HorizontalAlignment" Value="Left" />
                <Setter TargetName="LayoutRoot" Property="VerticalAlignment" Value="Top" />
                <Setter TargetName="PART_RestoreButton" Property="Style" Value="{StaticResource WindowMinimizeButtonStyle}" />
                <Setter TargetName="Header" Property="CornerRadius" Value="{telerik1:CrystalResource ResourceKey=CornerRadius}" />
            </Trigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition SourceName="IconContent" Property="Content" Value="{x:Null}" />
                    <Condition SourceName="IconContent" Property="ContentTemplate" Value="{x:Null}" />
                </MultiTrigger.Conditions>
                <Setter TargetName="IconContent" Property="Visibility" Value="Collapsed" />
            </MultiTrigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="ResizeMode" Value="CanResizeWithGrip" />
                    <Condition Property="WindowState" Value="Normal" />
                </MultiTrigger.Conditions>
                <Setter Property="MinHeight" Value="66" />
                <Setter TargetName="WindowResizeGrip" Property="Visibility" Value="Visible" />
            </MultiTrigger>
            <Trigger Property="mat:MaterialAssist.IsShadowDisabled" Value="True">
                <Setter TargetName="LayoutRoot" Property="Margin" Value="0" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
