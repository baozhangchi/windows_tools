            </Trigger>
            <Trigger Property="IsPressed" Value="True">
                <Setter Property="Foreground" Value="{telerik1:CrystalResource ResourceKey=IconBrush}" />
                <Setter Property="BorderBrush" Value="{telerik1:CrystalResource ResourceKey=PressedBrush}" />
            </Trigger>
            <Trigger Property="IsChecked" Value="True">
                <Setter Property="Foreground" Value="{telerik1:CrystalResource ResourceKey=IconBrush}" />
                <Setter Property="BorderBrush" Value="{telerik1:CrystalResource ResourceKey=BasicBrush}" />
                <Setter Property="mat:MaterialAssist.MouseOverBrush" Value="{telerik1:CrystalResource ResourceKey=PressedBrush}" />
                <Setter Property="mat:MaterialAssist.PressedBrush" Value="{telerik1:CrystalResource ResourceKey=PressedBrush}" />
            </Trigger>
        </Style.Triggers>
    </Style>
    <Style
        x:Key="BreadcrumbSplitButtonStyle"
        BasedOn="{StaticResource RadSplitButtonStyle}"
        TargetType="telerik:RadSplitButton">
        <Setter Property="mat:MaterialAssist.CornerRadius" Value="{telerik1:CrystalResource ResourceKey=CornerRadius}" />
        <Setter Property="mat:MaterialAssist.IsShadowDisabled" Value="True" />
        <Setter Property="CornerRadius" Value="0" />
        <Setter Property="BorderThickness" Value="0" />
        <Setter Property="IsBackgroundVisible" Value="False" />
        <Setter Property="VerticalAlignment" Value="Stretch" />
        <Setter Property="Margin" Value="0,1" />
        <Setter Property="Padding" Value="5,0" />
        <Setter Property="ButtonPartStyle" Value="{StaticResource BreadCrumbButtonStyle}" />
        <Setter Property="TogglePartStyle" Value="{StaticResource BreadCrumbToggleButtonStyle}" />
        <Setter Property="DropDownIndicatorVisibility" Value="Collapsed" />
        <Setter Property="IsTabStop" Value="False" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerik:RadSplitButton">
                    <Grid>
                        <Border
                            x:Name="BorderVisual"
                            Background="{x:Null}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}"
                            CornerRadius="{TemplateBinding CornerRadius}"
                            IsHitTestVisible="False"
                            SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" />
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="*" />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="Auto" />
                            </Grid.ColumnDefinitions>
                            <telerik:RadButton
                                x:Name="ButtonPart"
                                Grid.Row="1"
                                Grid.Column="1"
                                Content="{TemplateBinding Content}"
                                ContentStringFormat="{TemplateBinding ContentStringFormat}"
                                ContentTemplate="{TemplateBinding ContentTemplate}"
                                ContentTemplateSelector="{TemplateBinding ContentTemplateSelector}"
                                IsBackgroundVisible="{TemplateBinding IsBackgroundVisible}"
                                Style="{TemplateBinding ButtonPartStyle}" />
                            <telerik:RadToggleButton
                                x:Name="DropDownPart"
                                Grid.Row="1"
                                Grid.Column="2"
                                IsBackgroundVisible="{TemplateBinding IsBackgroundVisible}"
                                IsChecked="{Binding Path=IsOpen, Mode=TwoWay, RelativeSource={RelativeSource TemplatedParent}}"
                                Style="{TemplateBinding TogglePartStyle}"
                                Visibility="{TemplateBinding DropDownIndicatorVisibility}">
                                <telerik:RadToggleButton.Content>
                                    <telerik:RadGlyph
                                        x:Name="arrow"
                                        Foreground="{Binding Foreground, RelativeSource={RelativeSource AncestorType={x:Type telerik:RadToggleButton}}}"
                                        Glyph="{StaticResource GlyphArrowChevronRight}"
                                        IsHitTestVisible="False" />
                                </telerik:RadToggleButton.Content>
                            </telerik:RadToggleButton>
                        </Grid>
                        <Popup
                            x:Name="DropDownPopup"
                            AllowsTransparency="True"
                            Focusable="False"
                            HorizontalOffset="0"
                            Placement="Bottom"
                            PopupAnimation="{TemplateBinding PopupAnimation}"
                            StaysOpen="False"
                            VerticalOffset="2">
                            <Grid x:Name="PopupGrid" Margin="10,8">
                                <mat:Shadow
                                    x:Name="PopupShadow"
                                    Width="{TemplateBinding DropDownWidth}"
                                    Height="{TemplateBinding DropDownHeight}"
                                    Background="{telerik1:CrystalResource ResourceKey=AlternativeBrush}"
                                    CornerRadius="{TemplateBinding mat:MaterialAssist.CornerRadius}"
                                    ShadowDepth="{StaticResource PopupShadowDepth}"
                                    SnapsToDevicePixels="True" />
                                <Border
                                    x:Name="DropDownPopupBorder"
                                    Width="{TemplateBinding DropDownWidth}"
                                    Height="{TemplateBinding DropDownHeight}"
                                    MinWidth="3"
                                    MinHeight="3"
                                    MaxWidth="{TemplateBinding DropDownMaxWidth}"
                                    MaxHeight="{TemplateBinding DropDownMaxHeight}"
                                    Background="{telerik1:CrystalResource ResourceKey=AlternativeBrush}"
                                    BorderBrush="{telerik1:CrystalResource ResourceKey=ComplementaryBasicBrush}"
                                    BorderThickness="1"
                                    CornerRadius="{TemplateBinding mat:MaterialAssist.CornerRadius}">
                                    <ContentControl
                                        x:Name="DropDownPopupContent"
                                        HorizontalAlignment="Stretch"
                                        VerticalAlignment="Stretch"
                                        HorizontalContentAlignment="Stretch"
                                        VerticalContentAlignment="Stretch"
                                        Content="{TemplateBinding DropDownContent}"
                                        ContentTemplate="{TemplateBinding DropDownContentTemplate}"
                                        DataContext="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=DataContext}" />
                                </Border>
                            </Grid>
                        </Popup>
                    </Grid>
                    <ControlTemplate.Triggers>

                        <!--  enabled & opened  -->
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="IsEnabled" Value="True" />
                                <Condition Property="IsOpen" Value="True" />
                            </MultiTrigger.Conditions>
                            <Setter TargetName="arrow" Property="Glyph" Value="{StaticResource GlyphArrowChevronDown}" />
                        </MultiTrigger>
                        <Trigger SourceName="DropDownPart" Property="IsMouseOver" Value="True">
                            <Setter TargetName="ButtonPart" Property="Background" Value="{telerik1:CrystalResource ResourceKey=MainBrush}" />
                            <Setter TargetName="ButtonPart" Property="BorderBrush" Value="{telerik1:CrystalResource ResourceKey=BasicBrush}" />
                        </Trigger>
                        <Trigger SourceName="DropDownPart" Property="IsChecked" Value="True">
                            <Setter TargetName="ButtonPart" Property="Background" Value="{telerik1:CrystalResource ResourceKey=PressedBrush}" />
                            <Setter TargetName="ButtonPart" Property="BorderBrush" Value="{telerik1:CrystalResource ResourceKey=BasicBrush}" />
                        </Trigger>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition SourceName="ButtonPart" Property="IsMouseOver" Value="True" />
                                <Condition Property="IsOpen" Value="False" />
                            </MultiTrigger.Conditions>
                            <Setter TargetName="DropDownPart" Property="Background" Value="{telerik1:CrystalResource ResourceKey=AccentBrush}" />
                            <Setter TargetName="DropDownPart" Property="BorderBrush" Value="{telerik1:CrystalResource ResourceKey=AccentBasicBrush}" />
                            <Setter TargetName="DropDownPart" Property="Foreground" Value="{telerik1:CrystalResource ResourceKey=MarkerInvertedBrush}" />
                        </MultiTrigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style
        x:Key="BreadcrumbOverflowSplitButtonStyle"
        BasedOn="{StaticResource RadSplitButtonStyle}"
        TargetType="telerik:RadSplitButton">
        <Setter Property="mat:MaterialAssist.FocusBrush" Value="{x:Null}" />
        <Setter Property="mat:MaterialAssist.IsShadowDisabled" Value="True" />
        <Setter Property="CornerRadius" Value="0" />
        <Setter Property="Foreground" Value="{telerik1:CrystalResource ResourceKey=IconBrush}" />
        <Setter Property="BorderBrush" Value="{telerik1:CrystalResource ResourceKey=AlternativeBasicBrush}" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="IsBackgroundVisible" Value="False" />
        <Setter Property="IsButtonPartVisible" Value="False" />
        <Setter Property="MinWidth" Value="24" />
        <Setter Property="MaxWidth" Value="150" />
        <Setter Property="DropDownMaxHeight" Value="700" />
        <Setter Property="DropDownMaxWidth" Value="400" />
        <Setter Property="Padding" Value="0" />
        <Setter Property="Margin" Value="0,0,-1,0" />
        <Setter Property="TogglePartStyle" Value="{StaticResource BreadCrumbToggleButtonStyle}" />
        <Setter Property="IsTabStop" Value="False" />
    </Style>
    <Style
        x:Key="BreadcrumbTextSplitButton"
        BasedOn="{StaticResource RadSplitButtonStyle}"
        TargetType="telerik:RadSplitButton">
        <Setter Property="mat:MaterialAssist.CornerRadius" Value="{telerik1:CrystalResource ResourceKey=CornerRadius}" />
        <Setter Property="CornerRadius" Value="0" />
        <Setter Property="TogglePartStyle" Value="{StaticResource BreadCrumbHistoryToggleButtonStyle}" />
        <Setter Property="Background" Value="{x:Null}" />
        <Setter Property="BorderBrush" Value="Transparent" />
        <Setter Property="Padding" Value="3" />
        <Setter Property="IsTabStop" Value="False" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerik:RadSplitButton">
                    <Grid>
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="Auto" />
                            </Grid.ColumnDefinitions>
                            <ContentControl
                                x:Name="Content"
                                HorizontalContentAlignment="Stretch"
                                VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}"
                                Content="{TemplateBinding Content}"
                                ContentTemplate="{TemplateBinding ContentTemplate}"
                                Foreground="{TemplateBinding Foreground}"
                                IsTabStop="False" />
                            <telerik:RadToggleButton
                                x:Name="DropDownPart"
                                Grid.Column="1"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                Focusable="False"
                                IsChecked="{Binding Path=IsOpen, Mode=TwoWay, RelativeSource={RelativeSource TemplatedParent}}"
                                IsTabStop="False"
                                Style="{TemplateBinding TogglePartStyle}"
                                Visibility="{TemplateBinding DropDownIndicatorVisibility}">
                                <telerik:RadGlyph
                                    x:Name="DropDownIndicator"
                                    Glyph="{StaticResource GlyphArrowOverflowDown}"
                                    IsHitTestVisible="False" />
                            </telerik:RadToggleButton>
                        </Grid>
                        <Popup
                            x:Name="DropDownPopup"
                            AllowsTransparency="True"
                            Focusable="False"
                            IsOpen="{Binding Path=IsOpen, Mode=TwoWay, RelativeSource={RelativeSource TemplatedParent}}"
                            Placement="Bottom"
                            StaysOpen="False"
                            VerticalOffset="1">
                            <Grid Margin="10,8">
                                <mat:Shadow
                                    Background="{telerik1:CrystalResource ResourceKey=AlternativeBrush}"
                                    CornerRadius="{TemplateBinding mat:MaterialAssist.CornerRadius}"
                                    ShadowDepth="{StaticResource PopupShadowDepth}" />
                                <Border
                                    x:Name="DropDownPopupBorder"
                                    Width="{TemplateBinding DropDownWidth}"
                                    Height="{TemplateBinding DropDownHeight}"
                                    MaxWidth="{TemplateBinding DropDownMaxWidth}"
                                    MaxHeight="{TemplateBinding DropDownMaxHeight}"
                                    Background="{telerik1:CrystalResource ResourceKey=AlternativeBrush}"
                                    BorderBrush="{telerik1:CrystalResource ResourceKey=ComplementaryBasicBrush}"
                                    BorderThickness="1"
                                    CornerRadius="{TemplateBinding mat:MaterialAssist.CornerRadius}">
                                    <ContentPresenter
                                        x:Name="DropDownPopupContent"
                                        HorizontalAlignment="Stretch"
                                        VerticalAlignment="Stretch"
                                        Content="{TemplateBinding DropDownContent}"
                                        ContentTemplate="{TemplateBinding DropDownContentTemplate}"
                                        DataContext="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=DataContext}" />
                                </Border>
                            </Grid>
                        </Popup>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <ControlTemplate x:Key="BarItemControlTemplate" TargetType="telerikBreadcrumb:RadBreadcrumbBarItem">
        <Grid>
            <Border
                x:Name="FocusVisual"
                Background="{x:Null}"
                BorderBrush="{TemplateBinding mat:MaterialAssist.FocusBrush}"
                BorderThickness="{telerik1:CrystalResource ResourceKey=FocusThickness}"
                IsHitTestVisible="False"
                Visibility="Collapsed" />
            <Border
                x:Name="headerContainer"
                Background="{TemplateBinding Background}"
                BorderBrush="{TemplateBinding BorderBrush}"
                BorderThickness="{TemplateBinding BorderThickness}">
                <telerik:RadSplitButton
                    x:Name="SplitButton"
                    Command="{x:Static telerikBreadcrumb:RadBreadcrumbCommands.ClearSelectedItem}"
                    Content="{TemplateBinding Header}"
                    ContentTemplate="{TemplateBinding HeaderTemplate}"
                    ContentTemplateSelector="{TemplateBinding HeaderTemplateSelector}"
                    FontSize="{TemplateBinding FontSize}"
                    Foreground="{Binding Foreground, RelativeSource={RelativeSource TemplatedParent}}"
                    IsOpen="{Binding Path=IsPopupOpen, Mode=TwoWay, RelativeSource={RelativeSource TemplatedParent}}"
                    Style="{TemplateBinding SplitButtonStyle}">
                    <telerik:RadSplitButton.DropDownContent>
                        <ScrollViewer
                            MinWidth="148"
                            MaxWidth="270"
                            MaxHeight="250"
                            Margin="0"
                            Padding="0,5"
                            BorderThickness="0"
                            HorizontalScrollBarVisibility="Auto"
                            VerticalScrollBarVisibility="Auto">
                            <ItemsPresenter />
                        </ScrollViewer>
                    </telerik:RadSplitButton.DropDownContent>
                </telerik:RadSplitButton>
            </Border>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="SplitButtonVisibility">
                    <VisualState x:Name="SplitButtonVisible" />
                    <VisualState x:Name="SplitButtonHidden">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames
                                Storyboard.TargetName="SplitButton"
                                Storyboard.TargetProperty="Visibility"
                                Duration="0">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Collapsed</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="HasItems" Value="False">
                <Setter TargetName="SplitButton" Property="DropDownIndicatorVisibility" Value="Collapsed" />
            </Trigger>
            <Trigger Property="HasItems" Value="True">
                <Setter TargetName="SplitButton" Property="DropDownIndicatorVisibility" Value="Visible" />
            </Trigger>
            <Trigger Property="IsLinearMode" Value="True">
                <Setter TargetName="SplitButton" Property="DropDownIndicatorVisibility" Value="Collapsed" />
            </Trigger>
            <Trigger Property="IsButtonPartHidden" Value="True">
                <Setter TargetName="SplitButton" Property="IsButtonPartVisible" Value="False" />
            </Trigger>
            <Trigger Property="IsKeyboardFocusWithin" Value="True">
                <Setter TargetName="FocusVisual" Property="Visibility" Value="Visible" />
            </Trigger>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter TargetName="FocusVisual" Property="Visibility" Value="Collapsed" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <ControlTemplate x:Key="DropDownItemControlTemplate" TargetType="telerikBreadcrumb:RadBreadcrumbBarItem">
        <Grid>
            <Border
                x:Name="BorderVisual"
                Background="{TemplateBinding Background}"
                BorderBrush="{TemplateBinding BorderBrush}" />
            <Grid Background="Transparent">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="29" />
                    <ColumnDefinition Width="Auto" />
                </Grid.ColumnDefinitions>
                <Image
                    Margin="-3,0,0,0"
                    Source="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=Image}"
                    Style="{StaticResource BreadcrumbImageStyle}"
                    Visibility="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsIconVisible, Converter={StaticResource BooleanToVisibilityConverter}}" />
                <ContentControl
                    x:Name="contentControl"
                    Grid.Column="1"
                    Margin="{TemplateBinding Padding}"
                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                    Content="{TemplateBinding DropDownHeader}"
                    ContentTemplate="{TemplateBinding DropDownHeaderTemplate}"
                    ContentTemplateSelector="{TemplateBinding HeaderTemplateSelector}"
                    FontSize="{TemplateBinding FontSize}"
                    Foreground="{TemplateBinding Foreground}" />
            </Grid>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="ShouldHighlight" Value="True">
                <Setter Property="Background" Value="{telerik1:CrystalResource ResourceKey=AccentSelectedBrush}" />
                <Setter TargetName="contentControl" Property="Foreground" Value="{telerik1:CrystalResource ResourceKey=MarkerInvertedBrush}" />
            </Trigger>
            <Trigger Property="IsSelected" Value="True">
                <Setter TargetName="BorderVisual" Property="Background" Value="{telerik1:CrystalResource ResourceKey=AccentPressedBrush}" />
                <Setter TargetName="contentControl" Property="Foreground" Value="{telerik1:CrystalResource ResourceKey=MarkerInvertedBrush}" />
            </Trigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="ShouldHighlight" Value="True" />
                    <Condition Property="IsSelected" Value="True" />
                </MultiTrigger.Conditions>
                <Setter TargetName="BorderVisual" Property="Background" Value="{telerik1:CrystalResource ResourceKey=AccentMouseOverBrush}" />
            </MultiTrigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <Style x:Key="RadBreadcrumbStyle" TargetType="telerikNavigation:RadBreadcrumb">
        <Setter Property="mat:MaterialAssist.CornerRadius" Value="{telerik1:CrystalResource ResourceKey=CornerRadius}" />
        <Setter Property="FontFamily" Value="{telerik1:CrystalResource ResourceKey=FontFamily}" />
        <Setter Property="FontSize" Value="{telerik1:CrystalResource ResourceKey=FontSize}" />
        <Setter Property="Foreground" Value="{telerik1:CrystalResource ResourceKey=MarkerBrush}" />
        <Setter Property="Background" Value="{telerik1:CrystalResource ResourceKey=AlternativeBrush}" />
        <Setter Property="BorderBrush" Value="{telerik1:CrystalResource ResourceKey=AlternativeBasicBrush}" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="MinWidth" Value="150" />
        <Setter Property="MinHeight" Value="30" />
        <Setter Property="SplitButtonStyle" Value="{StaticResource BreadcrumbSplitButtonStyle}" />
        <Setter Property="OverflowButtonStyle" Value="{StaticResource BreadcrumbOverflowSplitButtonStyle}" />
        <Setter Property="DropDownItemControlTemplate" Value="{StaticResource DropDownItemControlTemplate}" />
        <Setter Property="BarItemControlTemplate" Value="{StaticResource BarItemControlTemplate}" />
        <Setter Property="SnapsToDevicePixels" Value="True" />
        <Setter Property="FocusVisualStyle" Value="{x:Null}" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikNavigation:RadBreadcrumb">
                    <Grid x:Name="outerGrid">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="28" />
                        </Grid.ColumnDefinitions>
                        <Border
                            x:Name="NormalVisual"
                            Grid.ColumnSpan="3"
                            Background="{TemplateBinding Background}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}"
                            SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" />
                        <Image
                            x:Name="image"
                            Margin="6,0"
                            Source="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=CurrentContainer.Image}"
                            Style="{StaticResource BreadcrumbImageStyle}"
                            Visibility="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsCurrentIconVisible, Converter={StaticResource BooleanToVisibilityConverter}}" />
                        <telerik:RadSplitButton
                            x:Name="historySplitButton"
                            Grid.Column="1"
                            Grid.ColumnSpan="2"
                            BorderThickness="{TemplateBinding BorderThickness}"
                            IsOpen="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsHistoryOpen, Mode=TwoWay}"
                            IsTabStop="False"
                            Style="{StaticResource BreadcrumbTextSplitButton}"
                            TogglePartStyle="{StaticResource BreadCrumbHistoryToggleButtonStyle}">
                            <telerik:RadSplitButton.Content>
                                <TextBox
                                    x:Name="pathTextBox"
                                    VerticalAlignment="Stretch"
                                    mat:MaterialAssist.FocusBrush="{x:Null}"
                                    mat:MaterialAssist.IsShadowDisabled="True"
                                    mat:MaterialAssist.MouseOverBrush="{x:Null}"
                                    AutomationProperties.Name="history"
                                    Background="{x:Null}"
                                    BorderBrush="{x:Null}"
                                    BorderThickness="0"
                                    FontSize="{TemplateBinding FontSize}"
                                    IsTabStop="False"
                                    Opacity="0" />
                            </telerik:RadSplitButton.Content>
                            <telerik:RadSplitButton.DropDownContent>
                                <ListBox
                                    x:Name="HistoryListBox"
                                    MinHeight="{StaticResource BreadcrumbListBoxMinHeight}"
                                    HorizontalContentAlignment="Stretch"
                                    VerticalContentAlignment="Stretch"
                                    mat:MaterialAssist.CornerRadius="{TemplateBinding mat:MaterialAssist.CornerRadius}"
                                    mat:MaterialAssist.ShadowDepth="Depth0"
                                    BorderThickness="0"
                                    FontSize="{TemplateBinding FontSize}"
                                    Foreground="{TemplateBinding Foreground}"
                                    ItemContainerStyle="{StaticResource BreadcrumbListBoxItemStyle}"
                                    ItemsSource="{TemplateBinding HistoryItems}"
                                    SelectedItem="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SelectedHistoryItem, Mode=TwoWay}" />
                            </telerik:RadSplitButton.DropDownContent>
                        </telerik:RadSplitButton>
                        <telerikBreadcrumb:RadBreadcrumbBar
                            x:Name="BreadcrumbBar"
                            Grid.Column="1"
                            Background="Transparent"
                            FontSize="{TemplateBinding FontSize}"
                            Foreground="{TemplateBinding Foreground}"
                            ItemTemplate="{TemplateBinding ItemTemplate}"
                            ItemTemplateSelector="{TemplateBinding ItemTemplateSelector}" />
                        <Border
                            x:Name="FocusVisual"
                            Grid.ColumnSpan="3"
                            Background="{x:Null}"
                            BorderBrush="{TemplateBinding mat:MaterialAssist.FocusBrush}"
                            BorderThickness="{telerik1:CrystalResource ResourceKey=FocusThickness}"
                            IsHitTestVisible="False"
                            Visibility="Collapsed" />
                        <Popup
                            x:Name="AutocompletePopup"
                            AllowsTransparency="True"
                            Focusable="False"
                            Placement="Bottom"
                            StaysOpen="False"
                            VerticalOffset="1">
                            <Grid Margin="10,8">
                                <mat:Shadow
                                    Background="{telerik1:CrystalResource ResourceKey=AlternativeBrush}"
                                    CornerRadius="{TemplateBinding mat:MaterialAssist.CornerRadius}"
                                    ShadowDepth="{StaticResource PopupShadowDepth}" />
                                <ListBox
                                    x:Name="AutoCompleteListBox"
                                    MinWidth="150"
                                    MinHeight="{StaticResource BreadcrumbListBoxMinHeight}"
                                    MaxWidth="650"
                                    MaxHeight="500"
                                    mat:MaterialAssist.CornerRadius="{TemplateBinding mat:MaterialAssist.CornerRadius}"
                                    BorderBrush="{telerik1:CrystalResource ResourceKey=ComplementaryBasicBrush}"
                                    FontSize="{TemplateBinding FontSize}"
                                    Foreground="{TemplateBinding Foreground}"
                                    ItemContainerStyle="{StaticResource BreadcrumbListBoxItemStyle}"
                                    ItemsSource="{TemplateBinding AutocompleteItems}"
                                    SelectedItem="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SelectedAutocompleteItem, Mode=TwoWay}" />
                            </Grid>
                        </Popup>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsFocused" Value="True">
                            <Setter TargetName="FocusVisual" Property="Visibility" Value="Visible" />
                        </Trigger>
                        <Trigger Property="IsTextModeEnabled" Value="False">
                            <Setter TargetName="historySplitButton" Property="Visibility" Value="Collapsed" />
                        </Trigger>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="IsTextModeEnabled" Value="True" />
                                <Condition Property="IsTextMode" Value="True" />
                            </MultiTrigger.Conditions>
                            <Setter TargetName="BreadcrumbBar" Property="Visibility" Value="Collapsed" />
                            <Setter TargetName="FocusVisual" Property="Visibility" Value="Visible" />
                            <Setter TargetName="pathTextBox" Property="Opacity" Value="1" />
                        </MultiTrigger>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="IsTextModeEnabled" Value="True" />
                                <Condition Property="IsTextMode" Value="False" />
                            </MultiTrigger.Conditions>
                            <Setter TargetName="BreadcrumbBar" Property="Visibility" Value="Visible" />
                        </MultiTrigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="RadBreadcrumbBarStyle" TargetType="telerikBreadcrumb:RadBreadcrumbBar">
        <Setter Property="IsTabStop" Value="False" />
        <Setter Property="ItemsPanel">
            <Setter.Value>
                <ItemsPanelTemplate>
                    <StackPanel Orientation="Horizontal" />
