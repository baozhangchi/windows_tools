                            <Setter TargetName="DropDownPopup" Property="Placement" Value="Top" />
                            <Setter TargetName="DropDownPopup" Property="VerticalOffset" Value="-2" />
                            <Setter TargetName="DropDownIndicator" Property="Glyph" Value="{StaticResource GlyphArrowChevronUp}" />
                        </Trigger>
                        <Trigger Property="DropDownPlacement" Value="Left">
                            <Setter TargetName="DropDownPopup" Property="Placement" Value="Left" />
                            <Setter TargetName="DropDownPopup" Property="HorizontalOffset" Value="2" />
