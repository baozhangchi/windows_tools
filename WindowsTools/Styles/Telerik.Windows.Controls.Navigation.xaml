    <Style
        x:Key="WindowButtonStyle"
        BasedOn="{StaticResource RadButtonStyle}"
        TargetType="telerik:RadButton">
        <Setter Property="Height" Value="16" />
        <Setter Property="Width" Value="16" />
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="BorderBrush" Value="{x:Null}" />
        <Setter Property="BorderThickness" Value="0" />
        <Setter Property="Margin" Value="10,0,0,0" />
        <Setter Property="Padding" Value="0" />
        <Setter Property="Foreground" Value="{telerik1:CrystalResource ResourceKey=IconBrush}" />
        <Setter Property="VerticalAlignment" Value="Center" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
        <Setter Property="HorizontalContentAlignment" Value="Center" />
        <Setter Property="IsTabStop" Value="False" />
        <Setter Property="FocusVisualStyle" Value="{x:Null}" />
        <Setter Property="UseLayoutRounding" Value="True" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerik:RadButton">
                    <Grid>
                        <Ellipse
                            Width="{TemplateBinding Width}"
                            Height="{TemplateBinding Height}"
                            Fill="{TemplateBinding Background}"
                            Stroke="{TemplateBinding BorderBrush}"
                            StrokeThickness="1" />
                        <ContentPresenter
                            x:Name="Content"
                            Margin="{TemplateBinding Padding}"
                            HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                            VerticalAlignment="{TemplateBinding VerticalContentAlignment}" />
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="Content" Property="Opacity" Value="{telerik1:CrystalResource ResourceKey=DisabledOpacity}" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style
        x:Key="WindowMinimizeButtonStyle"
        BasedOn="{StaticResource WindowButtonStyle}"
        TargetType="telerik:RadButton">
        <Setter Property="Background" Value="#FFFEB22E" />
        <Setter Property="BorderBrush" Value="#FFD69B1C" />
        <Style.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Background" Value="#FFD69B1C" />
            </Trigger>
            <Trigger Property="IsPressed" Value="True">
                <Setter Property="Background" Value="#FFD69B1C" />
            </Trigger>
        </Style.Triggers>
    </Style>
    <Style
        x:Key="WindowMaximizeButtonStyle"
        BasedOn="{StaticResource WindowButtonStyle}"
        TargetType="telerik:RadButton">
        <Setter Property="Background" Value="#FF62CD4B" />
        <Setter Property="BorderBrush" Value="#FF57B642" />
        <Style.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Background" Value="#FF57B642" />
            </Trigger>
            <Trigger Property="IsPressed" Value="True">
                <Setter Property="Background" Value="#FF57B642" />
            </Trigger>
        </Style.Triggers>
    </Style>
    <Style
        x:Key="WindowCloseButtonStyle"
        BasedOn="{StaticResource WindowButtonStyle}"
        TargetType="telerik:RadButton">
        <Setter Property="Background" Value="#FFE2515D" />
        <Setter Property="BorderBrush" Value="#FFC93630" />
        <Style.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="BorderBrush" Value="#FFC93630" />
                <Setter Property="Background" Value="{telerik1:CrystalResource ResourceKey=ValidationBrush}" />
            </Trigger>
            <Trigger Property="IsPressed" Value="True">
                <Setter Property="BorderBrush" Value="#FFC93630" />
                <Setter Property="Background" Value="{telerik1:CrystalResource ResourceKey=ValidationBrush}" />
            </Trigger>
        </Style.Triggers>
    </Style>
    <ControlTemplate x:Key="RadWindowTemplate" TargetType="telerikNavigation:RadWindow">
        <Grid x:Name="LayoutRoot">
            <mat:Shadow
                x:Name="Shadow"
                Background="{TemplateBinding Background}"
                CornerRadius="{TemplateBinding CornerRadius}"
                ShadowDepth="{TemplateBinding mat:MaterialAssist.ShadowDepth}" />
            <Border
                x:Name="WindowOuterBorder"
                BorderBrush="{TemplateBinding BorderBrush}"
                BorderThickness="{TemplateBinding BorderThickness}"
                CornerRadius="{TemplateBinding CornerRadius}">
                <Grid telerik:CornerRadiusHelper.ClipRadius="{Binding ElementName=WindowOuterBorder, Path=CornerRadius}" telerik:CornerRadiusHelper.ClipRadiusOffset="{TemplateBinding telerik:CornerRadiusHelper.ClipRadiusOffset}">
                    <AdornerDecorator>
                        <Grid Background="{TemplateBinding Background}">
                            <Grid.RowDefinitions>
                                <RowDefinition
                                    x:Name="WindowHeaderRow"
                                    Height="Auto"
                                    MinHeight="36" />
                                <RowDefinition Height="*" />
                            </Grid.RowDefinitions>
                            <Border
                                x:Name="Header"
                                Background="{telerik1:CrystalResource ResourceKey=HeaderBrush}"
                                BorderBrush="{telerik1:CrystalResource ResourceKey=BasicBrush}"
                                BorderThickness="0,0,0,1">
                                <Grid>
                                    <Border
                                        x:Name="HighlightBorder"
                                        Height="8"
                                        VerticalAlignment="Top"
                                        BorderBrush="{telerik1:CrystalResource ResourceKey=PrimaryBackgroundBrush}"
                                        BorderThickness="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=BorderThickness, Converter={StaticResource ThicknessToOrientedThicknessConverter}, ConverterParameter=LeftTopRight}"
                                        Opacity="0.5" />
                                    <telerikNavigation:WindowHeaderPanel
                                        x:Name="HeaderPanel"
                                        AlignButtonsToLeft="True"
                                        AlignTitleToCenter="True">
                                        <Grid Margin="10,0,0,0" telerikNavigation:WindowHeaderPanel.ContainerType="Title">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="Auto" />
                                                <ColumnDefinition Width="*" />
                                            </Grid.ColumnDefinitions>
                                            <ContentPresenter
                                                x:Name="IconContent"
                                                Grid.Column="0"
                                                Margin="{TemplateBinding IconMargin}"
                                                VerticalAlignment="Center"
                                                Content="{TemplateBinding Icon}"
                                                ContentTemplate="{TemplateBinding IconTemplate}" />
                                            <ContentPresenter
                                                x:Name="HeaderContent"
                                                Grid.Column="1"
                                                Margin="10,0"
                                                VerticalAlignment="Center"
                                                Content="{TemplateBinding Header}"
                                                ContentTemplate="{TemplateBinding HeaderTemplate}"
                                                ContentTemplateSelector="{TemplateBinding HeaderTemplateSelector}"
                                                SnapsToDevicePixels="True"
                                                TextBlock.FontFamily="{TemplateBinding FontFamily}"
                                                TextBlock.FontSize="{TemplateBinding FontSize}"
                                                TextBlock.Foreground="{TemplateBinding Foreground}">
                                                <ContentPresenter.Resources>
                                                    <Style TargetType="TextBlock">
                                                        <Setter Property="TextTrimming" Value="CharacterEllipsis" />
                                                    </Style>
                                                </ContentPresenter.Resources>
                                            </ContentPresenter>
                                        </Grid>
                                        <Border x:Name="PART_HeaderButtonsBorder" telerikNavigation:WindowHeaderPanel.ContainerType="Buttons">
                                            <Grid x:Name="HeaderButtons">
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition />
                                                    <ColumnDefinition />
                                                    <ColumnDefinition />
                                                    <ColumnDefinition />
                                                </Grid.ColumnDefinitions>
                                                <telerik:RadButton
                                                    x:Name="PART_MinimizeButton"
                                                    Grid.Column="3"
                                                    Command="{x:Static telerik:WindowCommands.Minimize}"
                                                    Style="{StaticResource WindowMinimizeButtonStyle}"
                                                    Visibility="{Binding IsEnabled, RelativeSource={RelativeSource Self}, Converter={StaticResource BooleanToVisibilityConverter}}">
                                                    <ToolTipService.ToolTip>
                                                        <ToolTip telerik:LocalizationManager.ResourceKey="Minimize" />
                                                    </ToolTipService.ToolTip>
                                                    <telerik:RadGlyph FontSize="12" Glyph="{StaticResource GlyphMinimize}" />
                                                </telerik:RadButton>
                                                <telerik:RadButton
                                                    x:Name="PART_RestoreButton"
                                                    Grid.Column="2"
                                                    Command="{x:Static telerik:WindowCommands.Restore}"
                                                    Style="{StaticResource WindowMaximizeButtonStyle}"
                                                    Visibility="{Binding IsEnabled, RelativeSource={RelativeSource Self}, Converter={StaticResource BooleanToVisibilityConverter}}">
