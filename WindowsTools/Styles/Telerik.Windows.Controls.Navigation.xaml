                            Opacity="0"
                            TextElement.Foreground="{telerik:CrystalResource ResourceKey=IconBrush}" />
                    </Grid>
                </Grid>
            </Grid>
            <telerikNavigation:NavigationViewSubItemsHost
                x:Name="PART_NavigationViewSubItemsHost"
                Grid.Row="1"
                Background="Transparent"
                DisplayMemberPath="{TemplateBinding DisplayMemberPath}"
                Focusable="False"
                ItemContainerStyle="{TemplateBinding ItemContainerStyle}"
                ItemTemplate="{TemplateBinding ItemTemplate}"
                ItemsSource="{TemplateBinding ItemsSource}"
                UseLayoutRounding="True"
                Visibility="Collapsed" />
        </Grid>
        <ControlTemplate.Triggers>
            <DataTrigger Binding="{Binding RelativeSource={RelativeSource Self}, Path=IsTopLevel}" Value="False">
                <Setter TargetName="ItemContentGrid" Property="Margin" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=CalculatedItemIndentation, Converter={StaticResource DoubleToThicknessConverter}, ConverterParameter=Left}" />
            </DataTrigger>
            <Trigger Property="IsExpanded" Value="True">
                <Setter TargetName="ExpandedIcon" Property="Opacity" Value="1" />
            </Trigger>
            <Trigger Property="IsExpanded" Value="False">
                <Setter TargetName="CollapsedIcon" Property="Opacity" Value="1" />
            </Trigger>
            <Trigger Property="IconVisibility" Value="Collapsed">
                <Setter TargetName="IconColumn" Property="Width" Value="0" />
            </Trigger>
            <Trigger Property="IsHighlighted" Value="True">
                <Setter Property="BorderBrush" Value="{StaticResource NavigationView_NavItemBorderBrush_MouseOver}" />
                <Setter TargetName="IconContent" Property="TextElement.Foreground" Value="{telerik:CrystalResource ResourceKey=MarkerInvertedBrush}" />
                <Setter TargetName="Content" Property="TextElement.Foreground" Value="{telerik:CrystalResource ResourceKey=MarkerInvertedBrush}" />
                <Setter TargetName="ExpandedIcon" Property="TextElement.Foreground" Value="{telerik:CrystalResource ResourceKey=MarkerInvertedBrush}" />
                <Setter TargetName="CollapsedIcon" Property="TextElement.Foreground" Value="{telerik:CrystalResource ResourceKey=MarkerInvertedBrush}" />
                <Setter TargetName="HighlightedVisual" Property="Opacity" Value="1" />
            </Trigger>
            <Trigger Property="IsSelected" Value="True">
                <Setter Property="BorderBrush" Value="{StaticResource NavigationView_NavItemBorderBrush_Selected}" />
                <Setter TargetName="IconContent" Property="TextElement.Foreground" Value="{telerik:CrystalResource ResourceKey=MarkerInvertedBrush}" />
                <Setter TargetName="Content" Property="TextElement.Foreground" Value="{telerik:CrystalResource ResourceKey=MarkerInvertedBrush}" />
                <Setter TargetName="ExpandedIcon" Property="TextElement.Foreground" Value="{telerik:CrystalResource ResourceKey=MarkerInvertedBrush}" />
                <Setter TargetName="CollapsedIcon" Property="TextElement.Foreground" Value="{telerik:CrystalResource ResourceKey=MarkerInvertedBrush}" />
                <Setter TargetName="SelectedVisual" Property="Opacity" Value="1" />
            </Trigger>
            <Trigger Property="IsPressed" Value="True">
                <Setter Property="BorderBrush" Value="{StaticResource NavigationView_NavItemBorderBrush_Selected}" />
                <Setter TargetName="IconContent" Property="TextElement.Foreground" Value="{telerik:CrystalResource ResourceKey=MarkerInvertedBrush}" />
                <Setter TargetName="Content" Property="TextElement.Foreground" Value="{telerik:CrystalResource ResourceKey=MarkerInvertedBrush}" />
                <Setter TargetName="ExpandedIcon" Property="TextElement.Foreground" Value="{telerik:CrystalResource ResourceKey=MarkerInvertedBrush}" />
                <Setter TargetName="CollapsedIcon" Property="TextElement.Foreground" Value="{telerik:CrystalResource ResourceKey=MarkerInvertedBrush}" />
                <Setter TargetName="PressedVisual" Property="Opacity" Value="1" />
            </Trigger>
            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding RelativeSource={RelativeSource Self}, Path=IsExpanded}" Value="False" />
                    <Condition Binding="{Binding RelativeSource={RelativeSource Self}, Path=HasSelectedSubItem}" Value="True" />
                </MultiDataTrigger.Conditions>
                <Setter Property="BorderBrush" Value="{StaticResource NavigationView_NavItemBorderBrush_Selected}" />
                <Setter TargetName="IconContent" Property="TextElement.Foreground" Value="{telerik:CrystalResource ResourceKey=MarkerInvertedBrush}" />
                <Setter TargetName="Content" Property="TextElement.Foreground" Value="{telerik:CrystalResource ResourceKey=MarkerInvertedBrush}" />
                <Setter TargetName="ExpandedIcon" Property="TextElement.Foreground" Value="{telerik:CrystalResource ResourceKey=MarkerInvertedBrush}" />
                <Setter TargetName="CollapsedIcon" Property="TextElement.Foreground" Value="{telerik:CrystalResource ResourceKey=MarkerInvertedBrush}" />
                <Setter TargetName="SelectedVisual" Property="Opacity" Value="1" />
            </MultiDataTrigger>
            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding RelativeSource={RelativeSource Self}, Path=IsExpanded}" Value="True" />
                    <Condition Binding="{Binding RelativeSource={RelativeSource Self}, Path=HasSelectedSubItem}" Value="True" />
                </MultiDataTrigger.Conditions>
                <Setter TargetName="SelectedVisual" Property="Opacity" Value="0" />
            </MultiDataTrigger>
            <Trigger Property="IsEnabled" Value="False">
                <Setter TargetName="ItemContentGrid" Property="Opacity" Value="{telerik1:CrystalResource ResourceKey=DisabledOpacity}" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
