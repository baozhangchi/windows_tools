    <ControlTemplate x:Key="RadNavigationViewTemplate" TargetType="telerikNavigation:RadNavigationView">
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition />
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto" />
                <RowDefinition />
            </Grid.RowDefinitions>
            <Rectangle
                x:Name="PART_DismissOverlay"
                Grid.RowSpan="2"
                Grid.Column="1"
                Panel.ZIndex="2"
                Fill="Transparent"
                Visibility="Collapsed" />
            <Border
                x:Name="HeaderBackground"
                Grid.ColumnSpan="2"
                Height="{TemplateBinding PaneHeaderHeight}"
                Padding="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=CompactPaneWidth, Converter={StaticResource DoubleToThicknessConverter}, ConverterParameter=Left}"
                Panel.ZIndex="1"
                Background="{TemplateBinding PaneHeaderBackground}"
                BorderBrush="{TemplateBinding BorderBrush}"
                BorderThickness="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=BorderThickness, Converter={StaticResource ThicknessToOrientedThicknessConverter}, ConverterParameter=TopLeftRight}"
                Visibility="Collapsed">
                <ContentPresenter
                    Margin="10,0"
                    VerticalAlignment="Center"
                    ContentSource="PaneHeader"
                    TextBlock.Foreground="{TemplateBinding PaneHeaderForeground}" />
            </Border>
            <telerik1:RadToggleButton
                x:Name="PaneToggleButton"
                Width="{TemplateBinding CompactPaneWidth}"
                Height="{TemplateBinding PaneHeaderHeight}"
                HorizontalAlignment="Left"
                Panel.ZIndex="2"
                AutomationProperties.Name="PaneToggleButton"
                Content="{TemplateBinding PaneToggleButtonContent}"
                ContentTemplate="{TemplateBinding PaneToggleButtonContentTemplate}"
                IsChecked="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsPaneOpen, Mode=TwoWay}"
                Style="{TemplateBinding PaneToggleButtonStyle}"
                Visibility="{TemplateBinding PaneToggleButtonVisibility}" />
            <Border
                x:Name="ContentBorder"
                Grid.RowSpan="2"
                Grid.Column="1"
                Background="{TemplateBinding Background}"
                BorderBrush="{TemplateBinding BorderBrush}"
                BorderThickness="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=BorderThickness, Converter={StaticResource ThicknessToOrientedThicknessConverter}, ConverterParameter=TopRightBottom}">
                <ContentPresenter
                    x:Name="PART_Content"
                    ContentSource="Content"
                    TextBlock.Foreground="{TemplateBinding Foreground}" />
            </Border>
            <ScrollViewer
                x:Name="PART_FlyoutOverlay"
                Grid.RowSpan="2"
                Grid.Column="1"
                HorizontalAlignment="Left"
                VerticalAlignment="Top"
                Panel.ZIndex="3"
                Background="{TemplateBinding PaneBackground}"
                BorderBrush="{TemplateBinding BorderBrush}"
                BorderThickness="1"
                Visibility="Collapsed" />
            <Grid
                x:Name="PART_PaneRoot"
                Grid.RowSpan="2"
                Grid.Column="0"
                Width="{TemplateBinding PaneWidth}"
                Panel.ZIndex="1"
                Background="{TemplateBinding PaneBackground}">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="Auto" />
                </Grid.RowDefinitions>
                <Border
                    x:Name="PaneRootBorder"
                    Grid.RowSpan="3"
                    BorderBrush="{telerik:CrystalResource ResourceKey=AlternativeBasicBrush}"
                    BorderThickness="{TemplateBinding BorderThickness}" />
                <Grid
                    Grid.Row="0"
                    Height="{TemplateBinding PaneHeaderHeight}"
                    Margin="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=CompactPaneWidth, Converter={StaticResource DoubleToThicknessConverter}, ConverterParameter=Left}">
                    <Border Background="{TemplateBinding PaneHeaderBackground}" BorderThickness="{TemplateBinding BorderThickness}" />
                    <ContentPresenter
                        Margin="10,0"
                        VerticalAlignment="Center"
                        ContentSource="PaneHeader"
                        TextBlock.Foreground="{TemplateBinding PaneHeaderForeground}" />
                </Grid>
                <ScrollViewer
                    x:Name="PART_ScrollViewer"
                    Grid.Row="1"
                    Padding="0"
                    Background="{x:Null}"
                    BorderThickness="0"
                    CanContentScroll="{TemplateBinding ScrollViewer.CanContentScroll}"
                    HorizontalScrollBarVisibility="Disabled"
                    VerticalScrollBarVisibility="{TemplateBinding ScrollViewer.VerticalScrollBarVisibility}">
                    <ItemsPresenter />
                </ScrollViewer>
                <Border
                    x:Name="PaneFooterContentBorder"
                    Grid.Row="2"
                    BorderBrush="{telerik:CrystalResource ResourceKey=AlternativeBasicBrush}">
                    <ContentPresenter ContentSource="PaneFooter" />
                </Border>
            </Grid>
        </Grid>
        <ControlTemplate.Triggers>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsPaneOpen" Value="True" />
                    <Condition Property="DisplayMode" Value="Compact" />
                </MultiTrigger.Conditions>
                <Setter TargetName="PART_DismissOverlay" Property="Visibility" Value="Visible" />
            </MultiTrigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsPaneOpen" Value="True" />
                    <Condition Property="DisplayMode" Value="Minimal" />
                </MultiTrigger.Conditions>
                <Setter TargetName="PART_DismissOverlay" Property="Visibility" Value="Visible" />
            </MultiTrigger>
            <Trigger Property="DisplayMode" Value="Compact">
                <Setter TargetName="ContentBorder" Property="Grid.Column" Value="0" />
                <Setter TargetName="ContentBorder" Property="Grid.ColumnSpan" Value="2" />
                <Setter TargetName="ContentBorder" Property="Margin" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=CompactPaneWidth, Converter={StaticResource DoubleToThicknessConverter}, ConverterParameter=Left}" />
            </Trigger>
            <Trigger Property="DisplayMode" Value="Minimal">
                <Setter TargetName="ContentBorder" Property="Grid.Row" Value="1" />
                <Setter TargetName="ContentBorder" Property="Grid.Column" Value="0" />
                <Setter TargetName="ContentBorder" Property="Grid.ColumnSpan" Value="2" />
                <Setter TargetName="ContentBorder" Property="BorderThickness" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=BorderThickness}" />
                <Setter TargetName="HeaderBackground" Property="Visibility" Value="Visible" />
                <Setter TargetName="PART_PaneRoot" Property="Panel.ZIndex" Value="0" />
            </Trigger>
            <Trigger SourceName="PART_FlyoutOverlay" Property="Visibility" Value="Visible">
                <Setter TargetName="PART_DismissOverlay" Property="Grid.Column" Value="0" />
                <Setter TargetName="PART_DismissOverlay" Property="Grid.ColumnSpan" Value="2" />
                <Setter TargetName="PART_DismissOverlay" Property="Panel.ZIndex" Value="3" />
                <Setter TargetName="PART_DismissOverlay" Property="Visibility" Value="Visible" />
            </Trigger>
            <Trigger SourceName="PaneToggleButton" Property="IsMouseOver" Value="True">
                <Setter TargetName="PaneToggleButton" Property="Foreground" Value="{telerik:CrystalResource ResourceKey=MarkerInvertedBrush}" />
            </Trigger>
            <Trigger SourceName="PaneToggleButton" Property="IsChecked" Value="True">
                <Setter TargetName="PaneToggleButton" Property="Foreground" Value="{telerik:CrystalResource ResourceKey=MarkerInvertedBrush}" />
            </Trigger>
            <Trigger SourceName="PaneToggleButton" Property="IsPressed" Value="True">
                <Setter TargetName="PaneToggleButton" Property="Foreground" Value="{telerik:CrystalResource ResourceKey=MarkerInvertedBrush}" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <Style x:Key="RadNavigationViewStyle" TargetType="telerikNavigation:RadNavigationView">
        <Setter Property="ScrollViewer.VerticalScrollBarVisibility" Value="Auto" />
        <Setter Property="PaneBackground" Value="{telerik:CrystalResource ResourceKey=AlternativeBrush}" />
        <Setter Property="PaneHeaderBackground" Value="{telerik:CrystalResource ResourceKey=AlternativeBrush}" />
        <Setter Property="PaneHeaderForeground" Value="{telerik:CrystalResource ResourceKey=MarkerBrush}" />
        <Setter Property="Foreground" Value="{telerik1:CrystalResource ResourceKey=MarkerBrush}" />
        <Setter Property="Background" Value="{telerik:CrystalResource ResourceKey=PrimaryBackgroundBrush}" />
        <Setter Property="BorderBrush" Value="{telerik:CrystalResource ResourceKey=AlternativeBasicBrush}" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="PaneHeaderHeight" Value="36" />
        <Setter Property="CompactPaneWidth" Value="40" />
        <Setter Property="ExpandedPaneWidth" Value="250" />
        <Setter Property="SubItemsIndentation" Value="20" />
        <Setter Property="PaneToggleButtonStyle" Value="{StaticResource PaneToggleButtonBaseStyle}" />
        <Setter Property="PaneToggleButtonContent" Value="{StaticResource GlyphMenu}" />
        <Setter Property="PaneToggleButtonContentTemplate">
            <Setter.Value>
                <DataTemplate>
                    <telerik1:RadGlyph Foreground="{Binding RelativeSource={RelativeSource AncestorType=telerik1:RadToggleButton}, Path=Foreground}" Glyph="{Binding}" />
                </DataTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Template" Value="{StaticResource RadNavigationViewTemplate}" />
        <Setter Property="animation:AnimationManager.AnimationSelector">
            <Setter.Value>
                <animation:AnimationSelector>
                    <animation:ResizeAnimation
                        AnimationName="ResizePaneAnimation"
                        ResizeMode="Horizontal"
                        TargetElementName="PART_PaneRoot"
                        Duration="0:0:0.2">
                        <animation:ResizeAnimation.Easing>
                            <ExponentialEase EasingMode="EaseOut" />
                        </animation:ResizeAnimation.Easing>
                    </animation:ResizeAnimation>
                    <animation:SlideAnimation
                        AnimationName="MinimalPaneOpenAnimation"
                        Direction="In"
                        Orientation="Horizontal"
                        PixelsToAnimate="250"
                        TargetElementName="PART_PaneRoot"
                        Duration="0:0:0.2" />
                    <animation:SlideAnimation
                        AnimationName="MinimalPaneCloseAnimation"
                        Direction="Out"
                        Orientation="Horizontal"
                        PixelsToAnimate="250"
                        TargetElementName="PART_PaneRoot"
                        Duration="0:0:0.2" />
                    <animation:ExpandCollapseAnimation
                        AnimationName="ExpandItemAnimation"
                        Direction="In"
                        SpeedRatio="2"
                        TargetElementName="PART_NavigationViewSubItemsHost" />
                    <animation:ExpandCollapseAnimation
                        AnimationName="CollapseItemAnimation"
                        Direction="Out"
                        SpeedRatio="2"
                        TargetElementName="PART_NavigationViewSubItemsHost" />
                    <animation:ExpandCollapseAnimation
                        AnimationName="FlyoutExpandItemAnimation"
                        Direction="In"
                        Orientation="Horizontal"
                        SpeedRatio="2"
                        TargetElementName="PART_NavigationViewSubItemsHost" />
                    <animation:ExpandCollapseAnimation
                        AnimationName="FlyoutCollapseItemAnimation"
                        Direction="Out"
                        Orientation="Horizontal"
                        SpeedRatio="2"
                        TargetElementName="PART_NavigationViewSubItemsHost" />
                </animation:AnimationSelector>
            </Setter.Value>
        </Setter>
    </Style>
    <ControlTemplate x:Key="RadNavigationViewItemTemplate" TargetType="telerikNavigation:RadNavigationViewItem">
        <Grid x:Name="PART_LayoutRoot">
            <Grid.RowDefinitions>
                <RowDefinition Height="*" />
                <RowDefinition />
            </Grid.RowDefinitions>
            <Grid x:Name="PART_ContainerGrid">
                <Border
                    x:Name="BackgroundVisual"
                    Background="{TemplateBinding Background}"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    BorderThickness="{TemplateBinding BorderThickness}"
                    CornerRadius="{TemplateBinding helpers:ThemeHelper.CornerRadius}"
                    SnapsToDevicePixels="True" />
                <Border
                    x:Name="SelectedVisual"
                    Background="{TemplateBinding helpers:ThemeHelper.CheckedBrush}"
                    BorderBrush="{TemplateBinding helpers:ThemeHelper.CheckedBrush}"
                    BorderThickness="{TemplateBinding BorderThickness}"
