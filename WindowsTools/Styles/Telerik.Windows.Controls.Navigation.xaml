                        <ColumnDefinition Width="Auto" />
                    </Grid.ColumnDefinitions>
                    <ContentPresenter
                        x:Name="IconContent"
                        ContentSource="Icon"
                        TextElement.Foreground="{telerik:CrystalResource ResourceKey=IconBrush}" />
                    <ContentPresenter
                        x:Name="Content"
                        Grid.Column="1"
                        Margin="{TemplateBinding Padding}"
                        HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                        VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                        RecognizesAccessKey="True"
                        TextElement.Foreground="{TemplateBinding Foreground}" />
                    <Grid
                        x:Name="PART_ExpandCollapseIconHost"
                        Grid.Column="2"
                        MinWidth="{TemplateBinding MinHeight}"
                        Background="Transparent">
                        <ContentPresenter
                            x:Name="ExpandedIcon"
                            Content="{TemplateBinding ExpandedIcon}"
                            ContentTemplate="{TemplateBinding ExpandedIconTemplate}"
