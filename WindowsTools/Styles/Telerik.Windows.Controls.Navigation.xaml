            <Ellipse
                Width="48"
                Height="48"
                Fill="{telerik1:CrystalResource ResourceKey=MainBrush}"
                Stroke="{telerik1:CrystalResource ResourceKey=BasicBrush}"
                StrokeThickness="1" />
            <TextBlock
                HorizontalAlignment="Center"
                VerticalAlignment="Center"
                FontFamily="SegoeUI"
                FontSize="32"
                Foreground="{telerik1:CrystalResource ResourceKey=AccentSelectedBrush}"
