    <Style
        x:Key="DesktopAlertCloseButtonStyle"
        BasedOn="{StaticResource DesktopAlertButtonsBaseStyle}"
        TargetType="telerik:RadButton">
        <Setter Property="IsTabStop" Value="False" />
