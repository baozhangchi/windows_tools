                                <Condition Property="IsChecked" Value="True" />
                            </MultiTrigger.Conditions>
                            <Setter TargetName="radioButtonBorder" Property="Fill" Value="{telerik1:CrystalResource ResourceKey=AccentMouseOverBrush}" />
