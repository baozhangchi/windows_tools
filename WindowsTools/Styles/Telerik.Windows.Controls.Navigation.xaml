    <Style x:Key="RadialMenuButtonStyle" TargetType="telerikRadialMenu:RadialMenuButton">
        <Setter Property="mat:MaterialAssist.ShadowDepth" Value="Depth2" />
        <Setter Property="FontFamily" Value="{telerik1:CrystalResource ResourceKey=FontFamily}" />
        <Setter Property="FontSize" Value="{telerik1:CrystalResource ResourceKey=FontSize}" />
        <Setter Property="FontWeight" Value="Normal" />
        <Setter Property="Foreground" Value="{telerik:CrystalResource ResourceKey=MarkerInvertedBrush}" />
        <Setter Property="Background" Value="{telerik:CrystalResource ResourceKey=AccentSelectedBrush}" />
        <Setter Property="BorderBrush" Value="Transparent" />
        <Setter Property="BorderThickness" Value="0" />
        <Setter Property="MinWidth" Value="0" />
        <Setter Property="Width" Value="50" />
        <Setter Property="Height" Value="50" />
        <Setter Property="VerticalAlignment" Value="Top" />
        <Setter Property="HorizontalAlignment" Value="Left" />
        <Setter Property="FocusVisualStyle" Value="{x:Null}" />
        <Setter Property="ContentTemplate">
            <Setter.Value>
                <DataTemplate>
                    <telerik:RadGlyph FontSize="24" Glyph="{StaticResource GlyphGear}" />
                </DataTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="BackContentTemplate">
            <Setter.Value>
                <DataTemplate>
                    <telerik:RadGlyph
                        Margin="0,4,0,0"
                        FontSize="24"
                        Glyph="{StaticResource GlyphArrowLeft}" />
                </DataTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikRadialMenu:RadialMenuButton">
                    <Grid x:Name="RootGrid">
                        <Grid>
                            <mat:Shadow
                                Background="{TemplateBinding Background}"
                                CornerRadius="25"
                                ShadowDepth="{TemplateBinding mat:MaterialAssist.ShadowDepth}" />
                            <Ellipse
                                x:Name="BackgroundGlyph"
                                Width="{TemplateBinding Width}"
                                Height="{TemplateBinding Height}"
                                Fill="{TemplateBinding Background}"
                                RenderTransformOrigin=".5 .5"
                                Stroke="{TemplateBinding BorderBrush}"
                                StrokeThickness="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=BorderThickness.Left}" />
                            <ContentPresenter
                                x:Name="NormalGlyph"
                                Margin="0,-1,0,0"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Content="{TemplateBinding Content}"
                                ContentTemplate="{TemplateBinding ContentTemplate}"
                                RenderTransformOrigin="0.5 0.5" />
                            <ContentPresenter
                                x:Name="BackGlyph"
                                Margin="0,-3,0,0"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                ContentTemplate="{Binding Path=BackContentTemplate, RelativeSource={RelativeSource AncestorType={x:Type telerikRadialMenu:RadialMenuButton}}}"
                                RenderTransformOrigin="0.5 0.5"
                                Visibility="Collapsed" />
                        </Grid>
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="NavigationStates">
                                <VisualState x:Name="Back">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="NormalGlyph" Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Collapsed</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="BackGlyph" Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Visible</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Home">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="NormalGlyph" Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Visible</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="BackGlyph" Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Collapsed</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter Property="Foreground" Value="{telerik:CrystalResource ResourceKey=MarkerInvertedBrush}" />
                            <Setter TargetName="BackgroundGlyph" Property="Fill" Value="{telerik:CrystalResource ResourceKey=AccentMouseOverBrush}" />
                            <Setter TargetName="BackgroundGlyph" Property="Stroke" Value="Transparent" />
                            <Setter Property="Background" Value="{telerik1:CrystalResource ResourceKey=PrimaryBackgroundBrush}" />
                        </Trigger>
                        <Trigger Property="IsPressed" Value="True">
                            <Setter Property="Foreground" Value="{telerik:CrystalResource ResourceKey=MarkerInvertedBrush}" />
                            <Setter TargetName="BackgroundGlyph" Property="Fill" Value="{telerik:CrystalResource ResourceKey=AccentPressedBrush}" />
                            <Setter TargetName="BackgroundGlyph" Property="Stroke" Value="Transparent" />
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="NormalGlyph" Property="Opacity" Value="{telerik1:CrystalResource ResourceKey=DisabledOpacity}" />
                            <Setter TargetName="BackGlyph" Property="Opacity" Value="{telerik1:CrystalResource ResourceKey=DisabledOpacity}" />
                        </Trigger>
                        <Trigger Property="FlowDirection" Value="RightToLeft">
                            <Setter Property="BackContentTemplate">
                                <Setter.Value>
                                    <DataTemplate>
                                        <telerik:RadGlyph
                                            Margin="0,4,0,0"
                                            FontSize="24"
                                            Glyph="{StaticResource GlyphArrowRight}" />
                                    </DataTemplate>
                                </Setter.Value>
                            </Setter>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="NavigationItemButtonStyle" TargetType="telerikRadialMenu:NavigationItemButton">
        <Setter Property="Foreground" Value="{telerik:CrystalResource ResourceKey=MarkerInvertedBrush}" />
        <Setter Property="Background" Value="{telerik:CrystalResource ResourceKey=AccentSelectedBrush}" />
        <Setter Property="BorderBrush" Value="{telerik:CrystalResource ResourceKey=AlternativeBrush}" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikRadialMenu:NavigationItemButton">
                    <Grid>
                        <Path
                            x:Name="PART_ArrowGlyph"
                            Width="14"
                            Height="10"
                            Margin="0,4,0,0"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Top"
                            Canvas.ZIndex="102"
                            Data="{telerik:RadGlyph Glyph={StaticResource GlyphArrowChevronUp}}"
                            Fill="{TemplateBinding Foreground}"
                            IsHitTestVisible="False"
                            Stretch="Fill" />
                        <Path
                            x:Name="PART_PathElement"
                            Canvas.ZIndex="100"
                            Fill="{TemplateBinding Background}"
                            Stroke="{TemplateBinding BorderBrush}"
                            StrokeThickness="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=BorderThickness.Left}" />
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="CommonStates">
                                <VisualState x:Name="Normal" />
                                <VisualState x:Name="MouseOver">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_PathElement" Storyboard.TargetProperty="Fill">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="{telerik:CrystalResource ResourceKey=AccentMouseOverBrush}" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_ArrowGlyph" Storyboard.TargetProperty="Fill">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="{telerik:CrystalResource ResourceKey=MarkerInvertedBrush}" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Disabled">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_PathElement" Storyboard.TargetProperty="Fill">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="{telerik:CrystalResource ResourceKey=AlternativeBrush}" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_ArrowGlyph" Storyboard.TargetProperty="Fill">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="{telerik:CrystalResource ResourceKey=IconBrush}" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <DoubleAnimation
                                            Storyboard.TargetName="PART_ArrowGlyph"
                                            Storyboard.TargetProperty="Opacity"
                                            From="0"
                                            To="{telerik1:CrystalResource ResourceKey=DisabledOpacity}"
                                            Duration="0:0:0.3" />
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
